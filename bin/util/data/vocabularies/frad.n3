	owl:inverseOf <http://iflastandards.info/ns/fr/frad/P2003> ;
	owl:propertyDisjointWith <http://iflastandards.info/ns/fr/frad/P3019> , <http://iflastandards.info/ns/fr/frad/P3020> .

<http://iflastandards.info/ns/fr/frad/P2030> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
	reg:status status:1001 ;
	reg:name "hasAccompanyingRelationship"@en ;
	rdfs:label "has accompanying relationship"@en ;
	skos:definition "Relates a work to another work that is a supplement to that work (i.e., a work that is intended to be used in conjunction with or to augment the related work, such as a teacher’s guide or incidental music to a play; or intended to complement or be a companion to another work, such as a set of maps to a video travelogue, a computer disk to accompany a textbook, a set of coloured plates to accompany a text, the equal parts of a kit)."@en ;
	a :Property ;
	rdfs:domain <http://iflastandards.info/ns/fr/frbr/frbrer/C1001> ;
	rdfs:range <http://iflastandards.info/ns/fr/frbr/frbrer/C1001> .

<http://iflastandards.info/ns/fr/frad/P4027> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
	reg:status status:1001 ;
	reg:name "hasAcronymOrInitialsOrAbbreviation"@en ;
	rdfs:label "has acronym or initials or abbreviation"@en ;
	skos:definition "Relates a name of a corporate body and an acronym or initials or an abbreviation by which that body is known."@en ;
	a :Property ;
	rdfs:domain <http://iflastandards.info/ns/fr/frad/C1011> ;
	rdfs:range <http://iflastandards.info/ns/fr/frad/C1011> ;
	rdfs:label "tiene acrónimo o iniciales o abreviaturas"@es ;
	owl:inverseOf <http://iflastandards.info/ns/fr/frad/P4026> ;
	owl:propertyDisjointWith <http://iflastandards.info/ns/fr/frad/P4032> , <http://iflastandards.info/ns/fr/frad/P4026> , <http://iflastandards.info/ns/fr/frad/P4025> .

<http://iflastandards.info/ns/fr/frad/P3018> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
	reg:status status:1001 ;
	reg:name "hasAddition"@en ;
	rdfs:label "has addition"@en ;
	skos:definition "Relates a controlled access point to a name, title, date, place, number, or other designation added to the base access point."@en ;
	a :Property ;
	rdfs:comment "Sometimes referred to as a \"qualifier\"."@en ;
	skos:scopeNote "Includes the fuller form of name added to the base access point. Includes the title of nobility, title of royalty, or ecclesiastical title added to the base access point. Includes dates added to the base access point. Includes the place name associated with a corporate body added to the base access point. Includes a number associated with a corporate body or a musical work added to the base access point. Includes a title of an adaptation or version of a work added to the base access point. Includes the name and/or number of a section or part of a work added to the base access point. Includes a term designating the form of a work added to the base access point. Includes a term designating language of expression added to the base access point. Includes a term designating the key in which a musical work was originally composed added to the base access point. Includes a term designating the medium of performance for which a musical work was originally conceived added to the base access point. Includes other designations associated with persons and corporate bodies added to the base access point."@en ;
	rdfs:domain <http://iflastandards.info/ns/fr/frad/C1001> ;
	rdfs:label "tiene adición"@es .

<http://iflastandards.info/ns/fr/frad/P3041> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
	reg:status status:1001 ;
	reg:name "hasAddressCorporateBody"@en ;
	rdfs:label "has address (corporate body)"@en ;
	skos:definition "Relates a corporate body to the current or former address of the corporate body's office(s)."@en ;
	a :Property ;
	skos:scopeNote "Includes addresses for the corporate body's headquarters and/or other offices. Includes street address, postal address, telephone number, e-mail address, etc. Includes the address of a World Wide Web site operated by the corporate body, about the corporate body or related to the corporate body."@en ;
	rdfs:domain <http://iflastandards.info/ns/fr/frad/C1003> ;
	owl:propertyDisjointWith <http://iflastandards.info/ns/fr/frad/P3043> , <http://iflastandards.info/ns/fr/frad/P3042> , <http://iflastandards.info/ns/fr/frad/P3040> .

<http://iflastandards.info/ns/fr/frad/P3035> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
	reg:status status:1001 ;
	reg:name "hasAddressPerson"@en ;
	rdfs:label "has address (person)"@en ;
	skos:definition "Relates a person to the current or former address of the person's place of residence, business, or employer."@en ;
	a :Property ;
	skos:scopeNote "Includes street address, postal address, telephone number, e-mail address, etc. Includes the address of a World Wide Web site operated by the person, about the person or related to the person."@en ;
	rdfs:domain <http://iflastandards.info/ns/fr/frbr/frbrer/C1005> ;
	rdfs:label "tiene dirección (persona)"@es .

<http://iflastandards.info/ns/fr/frad/P3034> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
	reg:status status:1001 ;
	reg:name "hasAffiliation"@en ;
	rdfs:label "has affiliation"@en ;
	skos:definition "Relates a person to a group with which the person is affiliated or has been affiliated through employment, membership, cultural identity, etc."@en ;
	a :Property ;
	rdfs:domain <http://iflastandards.info/ns/fr/frbr/frbrer/C1005> ;
	rdfs:label "tiene afiliación"@es .

<http://iflastandards.info/ns/fr/frad/P4002> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
	reg:status status:1001 ;
	reg:name "hasAlternateScriptForm"@en ;
	rdfs:label "has alternate script form"@en ;
	skos:definition "Relates controlled access points that are established as alternate script forms of the access point for the authorized forms of name for the same entity."@en ;
	a :Property ;
	rdfs:domain <http://iflastandards.info/ns/fr/frad/C1001> ;
	rdfs:range <http://iflastandards.info/ns/fr/frad/C1001> ;
	owl:propertyDisjointWith <http://iflastandards.info/ns/fr/frad/P4001> , <http://iflastandards.info/ns/fr/frad/P4003> .

<http://iflastandards.info/ns/fr/frad/P4025> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
	reg:status status:1001 ;
	reg:name "hasAlternativeLinguisticFormCorporateBody"@en ;
	rdfs:label "has alternative linguistic form (corporate body)"@en ;
	skos:definition "Relates a name of a corporate body and an alternative linguistic form of name by which that body is known."@en ;
	a :Property ;
	skos:scopeNote "Includes translations of the name for the corporate body."@en ;
	rdfs:domain <http://iflastandards.info/ns/fr/frad/C1011> ;
	rdfs:range <http://iflastandards.info/ns/fr/frad/C1011> ;
	rdfs:label "tiene forma lingüística alternativa (entidad corporativa)"@es ;
	owl:propertyDisjointWith <http://iflastandards.info/ns/fr/frad/P4026> , <http://iflastandards.info/ns/fr/frad/P4027> , <http://iflastandards.info/ns/fr/frad/P4032> .

<http://iflastandards.info/ns/fr/frad/P4024> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
	reg:status status:1001 ;
	reg:name "hasAlternativeLinguisticFormFamily"@en ;
	rdfs:label "has alternative linguistic form (family)"@en ;
	skos:definition "Relates a name of a family and an alternative linguistic form of name by which that family is known."@en ;
	a :Property ;
	rdfs:domain <http://iflastandards.info/ns/fr/frad/C1013> ;
	rdfs:range <http://iflastandards.info/ns/fr/frad/C1013> ;
	rdfs:label "tiene forma lingüística alternativa (familia)"@es .

<http://iflastandards.info/ns/fr/frad/P4034> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
	reg:status status:1001 ;
	reg:name "hasAlternativeLinguisticFormPerson"@en ;
	rdfs:label "has alternative linguistic form (person)"@en ;
	skos:definition "Relates a name of a person and an alternative linguistic form of name by which that person is known."@en ;
	a :Property ;
	skos:scopeNote "Includes translations of a name into other languages."@en ;
	rdfs:domain <http://iflastandards.info/ns/fr/frad/C1012> ;
	rdfs:range <http://iflastandards.info/ns/fr/frad/C1012> ;
	rdfs:label "tiene forma lingüística alternativa (persona)"@es .

<http://iflastandards.info/ns/fr/frad/P4028> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
	reg:status status:1001 ;
	reg:name "hasAlternativeLinguisticFormWork"@en ;
	rdfs:label "has alternative linguistic form (work)"@en ;
	skos:definition "Relates a name of a work and an alternative linguistic form of name by which that work is known."@en ;
	a :Property ;
	rdfs:domain <http://iflastandards.info/ns/fr/frad/C1014> ;
	rdfs:range <http://iflastandards.info/ns/fr/frad/C1014> ;
	rdfs:label "tiene forma lingüística alternativa (obra)"@es .

<http://iflastandards.info/ns/fr/frad/P2013> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
	reg:status status:1001 ;
	reg:name "hasAppellation"@en ;
	rdfs:label "has appellation"@en ;
	a :Property ;
	rdfs:domain <http://iflastandards.info/ns/fr/frad/C1010> ;
	skos:definition "Relates a bibliographic entity to a name."@en ;
	rdfs:range <http://iflastandards.info/ns/fr/frad/C1006> ;
	owl:inverseOf <http://iflastandards.info/ns/fr/frad/P2014> .

<http://iflastandards.info/ns/fr/frad/P4046> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
