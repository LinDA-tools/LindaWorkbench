	RegAp:name "isAnImitationExpressionOfExpression"@en ;
	RegAp:status <http://metadataregistry.org/uri/RegStatus/1001> .

frbrer:P2079 a owl:NamedIndividual ;
	skos:definition "Relates an expression to another expression that is a component of the expression."@en ;
	skos:scopeNote "The component expression may be intended to be used in the context of the prior expression or may not depend to any significant extent on the context provided by the prior expression."@en ;
	RegAp:name "hasPartExpression"@en ;
	RegAp:status <http://metadataregistry.org/uri/RegStatus/1001> .

frbrer:P2080 a owl:NamedIndividual ;
	skos:definition "Relates an expression to another expression of which it is a component."@en ;
