<http://purl.org/essglobal/vocab/v1.0/hasCost> a owl:ObjectProperty ;
	rdfs:label "Has Cost"@en , "Tem Custo"@pt ;
	dc:description "The cost for a particular product or service produced by an SSE initiative or network."@en , "O custo para um determinado produto ou serviço produzido por uma rede ou iniciativa ESS."@pt ;
	rdfs:isDefinedBy <http://purl.org/essglobal/vocab/v1.0/> ;
	dc:issued "2014-02-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
