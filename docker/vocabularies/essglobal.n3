	dc:description "If the Location of Sale is the headquarter of the SSE initiative?"@en , "Se o Local de Venda é a sede da iniciativa de ESS?"@pt ;
	rdfs:isDefinedBy <http://purl.org/essglobal/vocab/v1.0/> ;
	dc:issued "2014-02-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	dc:modified "2014-08-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	rdfs:range <http://www.w3.org/2001/XMLSchema#boolean> ;
	rdfs:domain <http://purl.org/essglobal/vocab/v1.0/LocationOfSale> .

<http://purl.org/essglobal/vocab/v1.0/isSSE> a owl:DatatypeProperty ;
	rdfs:label "Is SSE Initiative"@en , "É Iniciativa da ESS"@pt ;
	dc:description "If the Location of Sale is a place belonging to the SSE."@en , "Se o Local de Venda é um lugar pertencente à ESS."@en ;
	rdfs:isDefinedBy <http://purl.org/essglobal/vocab/v1.0/> ;
	dc:issued "2014-02-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	dc:modified "2014-08-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	rdfs:range <http://www.w3.org/2001/XMLSchema#boolean> ;
	rdfs:domain <http://purl.org/essglobal/vocab/v1.0/LocationOfSale> .

<http://purl.org/essglobal/vocab/v1.0/deliveryRangeDistance> a owl:DatatypeProperty ;
	rdfs:label "Delivery Range (distance)"@en , "Alcance da Entrega (distância)"@pt ;
	dc:description "Delivery range of product or service, in kilometers or miles."@en , "O alcance da entrega do produto ou serviço, em quilómetros ou milhas."@pt ;
	rdfs:isDefinedBy <http://purl.org/essglobal/vocab/v1.0/> ;
