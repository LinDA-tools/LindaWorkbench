	rdfs:isDefinedBy <http://www.w3.org/ns/locn#> ;
	rdfs:domain locn:Address ;
	rdfs:range rdfs:Literal ;
	dcterms:identifier "locn:postCode" .

locn:geographicName a rdf:Property ;
	rdfs:label "geographic name"@en ;
	rdfs:comment """<p xmlns=\"http://purl.org/oslo/ns/localgov#\">A geographic name is a proper noun applied to a spatial object. Taking the example used in the <a xmlns=\"http://purl.org/oslo/ns/localgov#\" href=\"http://inspire.jrc.ec.europa.eu/documents/Data_Specifications/INSPIRE_DataSpecification_GN_v3.0.1.pdf\">INSPIRE document</a> (page 18), the following are all valid geographic names for the Greek capital:</p>
    <ul xmlns=\"http://purl.org/oslo/ns/localgov#\"><li xmlns=\"http://purl.org/oslo/ns/localgov#\">A?n?a (the Greek endonym written in the Greek script)</li><li xmlns=\"http://purl.org/oslo/ns/localgov#\">Athina (the standard Romanisation of the endonym)</li><li xmlns=\"http://purl.org/oslo/ns/localgov#\">Athens (the English language exonym)</li></ul>
    <p xmlns=\"http://purl.org/oslo/ns/localgov#\">For INSPIRE-conformant data, provide the metadata for the geographic name using a skos:Concept as a datatype.</p>"""^^rdf:XMLLiteral ;
	rdfs:isDefinedBy <http://www.w3.org/ns/locn#> ;
	wdrs:describedby <https://joinup.ec.europa.eu/system/files/project/Core_Vocabularies-Business_Location_Person-Specification-v0.3_0.zip> ;
	rdfs:domain dcterms:Location ;
	rdfs:range rdf:Literal ;
	dcterms:identifier "locn:geographicName" .

person:patronymicName a rdf:Property ;
	rdfs:label "patronymic name"@en ;
	rdfs:comment """Patronymic names are important in some countries. Iceland 
    does not have a concept of family name in the way that many other European countries 
    do, for example. In Bulgaria and Russia, patronymic names are in every day usage, 
    for example, the \"Sergeyevich\" (?????????) in \"Mikhail Sergeyevich Gorbachev\" 
    (?????? ????????? ????????)."""@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/person#> ;
	dcterms:identifier "person:patronymicName" .

