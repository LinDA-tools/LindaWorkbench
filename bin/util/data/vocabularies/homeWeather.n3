                           owl:onClass                  WeatherOntology:Wind ;
                           owl:onProperty               WeatherOntology:hasWeatherPhenomenon
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  WeatherOntology:Humidity ;
                           owl:onProperty               WeatherOntology:hasWeatherPhenomenon
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
