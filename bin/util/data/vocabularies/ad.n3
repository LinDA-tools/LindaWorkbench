<http://schemas.talis.com/2005/address/schema#streetAddress> a owl:DatatypeProperty ;
	rdfs:label "streetAddress"@en ;
	skos:definition "the building name and number and street name"@en ;
	rdfs:comment "For example: \"13 Queen Street\""@en ;
	rdfs:isDefinedBy <http://schemas.talis.com/2005/address/schema#> ;
	dct:issued "2006-06-27" .

<http://schemas.talis.com/2005/address/schema#localityName> a owl:DatatypeProperty ;
	rdfs:label "localityName"@en ;
	skos:definition "the name of the city, town, village or other geopolitical region for which the streetAddress is unambiguous"@en ;
	rdfs:comment "For example: \"Santiago 1400\""@en ;
	rdfs:isDefinedBy <http://schemas.talis.com/2005/address/schema#> ;
	dct:issued "2006-06-27" .

<http://schemas.talis.com/2005/address/schema#regionName> a owl:DatatypeProperty ;
	rdfs:label "regionName"@en ;
	skos:definition "the name of the region within the country, principality or national entity"@en ;
	rdfs:comment "For example: \"Alabama\""@en ;
	rdfs:isDefinedBy <http://schemas.talis.com/2005/address/schema#> ;
	dct:issued "2006-06-27" .

<http://schemas.talis.com/2005/address/schema#countryName> a owl:DatatypeProperty ;
	rdfs:label "countryName"@en ;
	skos:definition "the name of the country, principality or national entity"@en ;
	rdfs:comment "For example: \"Egypt\""@en ;
	rdfs:isDefinedBy <http://schemas.talis.com/2005/address/schema#> ;
	dct:issued "2006-06-27" .

<http://schemas.talis.com/2005/address/schema#postalCode> a owl:DatatypeProperty ;
	rdfs:label "postalCode"@en ;
	skos:definition "the postcode or zipcode of the address"@en ;
	rdfs:comment "For example: \"NN14 7DS\""@en ;
	rdfs:isDefinedBy <http://schemas.talis.com/2005/address/schema#> ;
	dct:issued "2006-06-27" .

<http://schemas.talis.com/2005/address/schema#tel> a owl:DatatypeProperty ;
	rdfs:label "tel"@en ;
	skos:definition "a telephone number which can be used to contact a person at the address"@en ;
	rdfs:comment "The number must be in standard international format. For example: \"+43 1 53410437\""@en ;
	rdfs:isDefinedBy <http://schemas.talis.com/2005/address/schema#> ;
	dct:issued "2006-06-27" .

<http://schemas.talis.com/2005/address/schema#fax> a owl:DatatypeProperty ;
	rdfs:label "fax"@en ;
	skos:definition "a fax number which can be used to contact a person at the address"@en ;
	rdfs:comment "The number must be in standard international format. For example: \"+43 1 53410434\""@en ;
	rdfs:isDefinedBy <http://schemas.talis.com/2005/address/schema#> ;
	dct:issued "2006-06-27" .

