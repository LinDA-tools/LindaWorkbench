:designs-experiments a owl:NamedIndividual , :IntellectualContribution ;
	rdfs:label "designs experiments"@en ;
	rdfs:comment "The contribution of experimental design and planning."@en .

:develops-methodology a owl:NamedIndividual , :ExperimentalContribution ;
