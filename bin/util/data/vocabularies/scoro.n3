dc:creator a owl:AnnotationProperty .

:percentageFTE a owl:AnnotationProperty ;
	rdfs:label "percentage full-time equivalent"@en ;
	rdfs:comment "A data property permitting specification of the proportion of a person's full time activity that is dedicated to a particular role (e.g. for part-time employment, 50% FTE)."@en ;
	rdfs:domain pro:RoleInTime .

rdfs:label a owl:AnnotationProperty .

dc:description a owl:AnnotationProperty .

rdfs:comment a owl:AnnotationProperty .

owl:versionInfo a owl:AnnotationProperty .

rdfs:seeAlso a owl:AnnotationProperty .

dc:contributor a owl:AnnotationProperty .

:withJobTitle a owl:AnnotationProperty ;
	rdfs:label "with job title"@en ;
	rdfs:comment "A data property connecting an agent's role in time to a job title that characterizes that role.  This data property complements the definition of that role in time specified by scoro:Role.  For example, for the scoro:OrganizationalRole 'Head of Department', the specific Job Title might be 'Linacre Professor of Zoology' or 'Regius Professor of Moral Philosophy'."@en ;
	rdfs:domain pro:RoleInTime .

dc:date a owl:AnnotationProperty .

:isEqualToRoleInTIme a owl:AnnotationProperty .

dc:title a owl:AnnotationProperty .

dc:rights a owl:AnnotationProperty .

:hasContributionContext a owl:ObjectProperty ;
	rdfs:label "has contribution context"@en ;
	rdfs:comment "A property relating a contribution situation to an entity representing the context for that situation."@en ;
	rdfs:domain :ContributionSituation ;
	rdfs:subPropertyOf tvc:withinContext .

:isContributionIn a owl:ObjectProperty ;
	rdfs:label "is contribution in"@en ;
	rdfs:comment "The property relating the definition of a specific contribution to the contribution situation of an agent making that contribution."@en ;
	rdfs:domain :Contribution ;
	rdfs:range :ContributionSituation ;
	owl:inverseOf :withContribution ;
	rdfs:subPropertyOf timeindexedsituation:hasTimeIndexedSetting .

:isEqualToContributionSituation a owl:ObjectProperty , owl:SymmetricProperty , owl:TransitiveProperty ;
	rdfs:label "is equal to contribution situation"@en ;
	rdfs:comment "This property allows the link of two contribution situations that are equals for particular aspects (i.e. for type of contribution, contribution effort and context)."@en ;
	rdfs:range :ContributionSituation ;
	rdfs:domain :ContributionSituation .

:isEqualToRoleInTime a owl:ObjectProperty ;
	rdfs:label "is equal to role in time"@en ;
	rdfs:comment "This property allows the link of two roles in time as being equal, and is particularly useful for specifying that two principal authorship roles are equal."@en ;
	rdfs:domain pro:RoleInTime ;
	rdfs:range pro:RoleInTime .

:isMadeBy a owl:ObjectProperty ;
	rdfs:label "is made by"@en ;
	rdfs:comment "A property relating a contribution situation that an agent makes, to that agent."@en ;
	rdfs:domain :ContributionSituation ;
	owl:inverseOf :makesContribution ;
	rdfs:subPropertyOf timeindexedsituation:forEntity ;
	rdfs:range foaf:Agent .

:isRelatedToContributionSituation a owl:ObjectProperty ;
	rdfs:label "is related to contribution situation"@en ;
	rdfs:comment "A property relating an entity to the contribution situation for which that entity provides the context."@en ;
	rdfs:range :ContributionSituation ;
	owl:inverseOf :hasContributionContext ;
	rdfs:subPropertyOf timeindexedsituation:hasTimeIndexedSetting .

:makesContribution a owl:ObjectProperty ;
	rdfs:label "makes contribution"@en ;
	rdfs:comment "An object property linking an agent to a contribution.  This is done via the indirection: foaf:Agent scoro:makesContribution [ a scoro:ContributionSituation ; scoro:withContribution scoro:Contribution ; scoro:withEffort scoro:ContributionEffort ] .  The ontology permits one to specify the time period over which a contribution is made, and the other contextual entities to which that agent's contribution relates."@en ;
	rdfs:range :ContributionSituation ;
	rdfs:subPropertyOf tvc:hasValue ;
	rdfs:domain foaf:Agent .

:withContribution a owl:ObjectProperty , owl:FunctionalProperty ;
	rdfs:label "with contribution"@en ;
	rdfs:comment "An object property connecting an agent's contribution situation to a definition of the contribution made by this agent, specified as an instance of the class scoro:Contribution or of one of its sub-classes."@en ;
	rdfs:range :Contribution ;
	rdfs:domain :ContributionSituation ;
	rdfs:subPropertyOf tvc:withValue .

:withEffort a owl:ObjectProperty ;
	rdfs:label "with contribution effort"@en ;
	rdfs:comment "An object property connecting an agent's contribution with respect to a particular entity to the amount of effort exerted by this agent in making that contribution, defined by individuals of the class scoro:ContributionEffort."@en ;
	rdfs:range :ContributionEffort ;
	rdfs:domain :ContributionSituation ;
	rdfs:subPropertyOf timeindexedsituation:forEntity .

tvc:hasValue a owl:ObjectProperty .

tvc:withValue a owl:ObjectProperty .

tvc:withinContext a owl:ObjectProperty .

timeindexedsituation:forEntity a owl:ObjectProperty .

timeindexedsituation:hasTimeIndexedSetting a owl:ObjectProperty .

:hasORCID a owl:DatatypeProperty ;
	rdfs:label "has ORCID"@en ;
	rdfs:comment "A data property relating an individual to his/her Open Researcher and Contributor ID (ORCID), an alphanumeric identifier for individual scholars described at http://about.orcid.org/."@en ;
	dc:description "owl:equivalentProperty <http://purl.org/spar/frapo/hasORCID> ."@en ;
	rdfs:subPropertyOf :hasPersonalIdentifier ;
	rdfs:domain foaf:Person .

:hasPersonalIdentifier a owl:DatatypeProperty ;
	rdfs:label "has personal identifier"@en ;
	dc:description "owl:equivalentProperty <http://purl.org/spar/datacite/hasPersonalIdentifier> ."@en , "owl:equivalentProperty <http://purl.org/spar/frapo/hasPersonalIdentifier> ."@en ;
	rdfs:comment "A data property specifying a unique and unambiguous identifier for an individual person within a given context, specified by the object property scoro:usesPersonalIdentifierScheme and an individual member of the class scoro:PersonalIdentifierScheme."@en ;
	rdfs:domain foaf:Person .

:percentageFTE a owl:DatatypeProperty .

:withJobTitle a owl:DatatypeProperty .

pro:Role a owl:Class .

pro:RoleInTime a owl:Class .

:AuthorshipContribution a owl:Class ;
	rdfs:label "authorship contribution"@en ;
	rdfs:subClassOf :Contribution ;
	rdfs:comment "Defines the type or nature of a contribution made with respect to the authorship of a publication such as a journal article.  Individual members of this class are used to specify particular authorship contributions."@en .

:AuthorshipRole a owl:Class ;
	rdfs:label "authorship role"@en ;
	rdfs:subClassOf pro:Role ;
	rdfs:comment "A role that a person might hold as an author of a publication or the creator of a work.  Individual members of this class are used to specify particular roles."@en .

:ComputationalAgent a owl:Class ;
	rdfs:label "computational agent"@en ;
	rdfs:subClassOf foaf:Agent ;
	rdfs:comment "A computer system or software program whose action has an effect on external events, for example a laboratory robot that undertakes your experiments, or the financial control system that controls an institution's payrole, or that, following programmed business rules, prevents the principal investigator from placing an order for research equipment because there is insufficient money left in the project budget."@en .

:Contribution a owl:Class ;
	rdfs:label "contribution"@en ;
	rdfs:subClassOf owl:Thing ;
	owl:disjointWith :ContributionEffort ;
	rdfs:comment "Defines the type or nature of a contribution made by an agent.  Individual members of this class or its sub-classes are used to specify particular contributions."@en .

:ContributionEffort a owl:Class ;
	rdfs:label "contribution effort"@en ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment "Defines the effort expended in making a contribution."@en .

:ContributionSituation a owl:Class ;
	rdfs:label "contribution situation"@en ;
	rdfs:subClassOf tvc:ValueInTime , _:node17ru5mbl8x1 .

_:node17ru5mbl8x1 a owl:Restriction ;
	owl:onProperty :withContribution ;
	owl:someValuesFrom :Contribution .

:ContributionSituation dc:description """A contribution situation describes always:
- a particular contribution C
- a particular agent A making contribution C
- a particular time interval T in which A makes C (optional)
- a particular contribution effort E 
- one or more entities (e.g. an investigation or data) that defines a context for C contributed by A. (At least one contextual entity should be specified for a particular contribution)."""@en ;
	rdfs:comment "The situation an agent has of making a contribution to realizing a goal or completing a task relating to a scholarly activity or entity such as journal article.  The type or nature of the contribution is defined by the object of the object property scoro:withContribution, which is an individual in the class scoro:Contribution or one of its sub-classes.  The entity to which the contribution relates is specified using the object property scoro:relatesToEntity or one of its sub-properties, and the effort expended in making that contribution is specified using the object property scoro:withContributionEffort."@en .

:DataRole a owl:Class ;
	rdfs:label "data role"@en ;
	rdfs:subClassOf pro:Role ;
	rdfs:comment "A role that an agent might hold with respect to data.  Individual members of this class are used to specify particular roles."@en .

:EducationalRole a owl:Class ;
	rdfs:label "educational role"@en ;
	rdfs:subClassOf pro:Role ;
	rdfs:comment "A role that an agent might hold with respect to an educational organization or activity.  Individual members of this class are used to specify particular educational roles."@en .

:ExperimentalContribution a owl:Class ;
	rdfs:label "experimental contribution"@en ;
	rdfs:subClassOf :Contribution ;
	rdfs:comment "Defines the type or nature of a practical or experimental contribution made to the research.  Individual members of this class are used to specify particular contributions."@en .

:FinancialRole a owl:Class ;
	rdfs:label "financial role"@en ;
	rdfs:subClassOf pro:Role ;
	rdfs:comment "A role that an agent might hold with respect to academic and project finance.  Individual members of this class are used to specify particular roles."@en .

:IntellectualContribution a owl:Class ;
	rdfs:label "intellectual contribution"@en ;
	rdfs:subClassOf :Contribution ;
	rdfs:comment "Defines the type or nature of an intellectual contribution made to the research described in a journal article.  Individual members of this class are used to specify particular contributions."@en .

:InvestigationRole a owl:Class ;
	rdfs:label "investigation role"@en ;
	rdfs:subClassOf pro:Role ;
	rdfs:comment "A role that an agent might hold with respect to an investigation.  Individual members of this class are used to specify particular roles."@en .

:OrganizationalContribution a owl:Class ;
	rdfs:label "organizational contribution"@en ;
	rdfs:subClassOf :Contribution ;
	rdfs:comment "Defines the type or nature of an organizational or administrative contribution.  Individual members of this class are used to specify particular contributions."@en .

:OrganizationalRole a owl:Class ;
	rdfs:label "organizational role"@en ;
	rdfs:subClassOf pro:Role ;
	rdfs:comment "A role that an agent might hold with respect to an organization and its administration, particularly within an academic context.  Individual members of this class are used to specify particular roles."@en .

:ProjectRole a owl:Class ;
	rdfs:label "project role"@en ;
	rdfs:subClassOf pro:Role ;
	rdfs:comment "A role that an agent might hold with respect to a project.  Individual members of this class are used to specify particular roles."@en .

tvc:ValueInTime a owl:Class .

foaf:Agent a owl:Class .

foaf:Person a owl:Class ;
	rdfs:label "person"@en .

pro:author a owl:NamedIndividual , :AuthorshipRole .

:access-provider a owl:NamedIndividual , :DataRole ;
	rdfs:label "access provider"@en ;
	rdfs:comment "The role of an agent who provides access to a resource, for example an online data repository."@en .

:accountant a owl:NamedIndividual , :FinancialRole ;
	rdfs:label "accountant"@en ;
	rdfs:comment "The role held by a person whose job is to manage financial accounts, for example the accounts of an organization, or of a particular funded project or activity of that organization."@en .

:administrator a owl:NamedIndividual , :OrganizationalRole ;
	rdfs:label "administrator"@en ;
	rdfs:comment "The role held by an agent responsible for the day-to-day management and running of an organization or activity."@en .

:affiliate a owl:NamedIndividual , :OrganizationalRole ;
	rdfs:label "affiliate"@en ;
	dc:description """The affiliation of a person to an organization or institution can be specified by using the property pro:relatesToEntity, as shown in the following example:

foaf:Person scoro:holdsOrganizationalRole [ pro:withRole pro:affiliate ; pro:relatesToEntity _:aff ] .

_:aff a foaf:Organization ; vcard:extended-address 'Department of Zoology, University of Oxford, United Kingdom' ."""@en ;
	rdfs:comment "The role an agent has of being affiliated to some organization or institution in a context of some entity or activity, for example a document, a project or an internship."@en .

:agent a owl:NamedIndividual , :OrganizationalRole ;
	rdfs:label "agent"@en ;
	rdfs:comment "An agent that acts on behalf of another agent."@en .

:analyses-data a owl:NamedIndividual , :IntellectualContribution ;
	rdfs:label "analyses data"@en ;
	rdfs:comment "The contribution of data analysis.  This might include statistical analysis, bioinformatics analysis, or image analysis."@en .

:approves-final-manuscript a owl:NamedIndividual , :AuthorshipContribution ;
	rdfs:label "approves final manuscript"@en ;
	rdfs:comment "The contribution of approving the final version of the manuscript, prior to submission for publication."@en .

:article-guarantor a owl:NamedIndividual , :AuthorshipRole ;
	rdfs:label "article guarantor"@en ;
	rdfs:comment "The role of a person who takes responsibility for the integrity of the work as a whole, from the inception of the research investigation to the published research article."@en .

:auditor a owl:NamedIndividual , :FinancialRole ;
	rdfs:label "auditor"@en ;
	rdfs:comment "The role held by a person who conducts a formal audit of financial accounts."@en .

:builds-and-or-maintains-instruments a owl:NamedIndividual , :ExperimentalContribution ;
	rdfs:label "builds and/or maintains instruments"@en ;
	rdfs:comment "The contribution of building instruments, equipment or scientific apparatus and/or maintaining the same in a workable condition for use in experiments."@en .

:chief-executive-officer a owl:NamedIndividual , :OrganizationalRole ;
	rdfs:label "CEO"@en , "chief executive officer"@en ;
	rdfs:comment "The role held by a person who is the chief executive officer of an organization, responsible for directing and managing the business of that organization."@en .

:chief-scientist a owl:NamedIndividual , :InvestigationRole ;
	rdfs:label "chief scientist"@en ;
	rdfs:comment "A senior scientist who takes a leadership role in an organization or group."@en .

:chief-technology-officer a owl:NamedIndividual , :OrganizationalRole ;
	rdfs:label "CTO"@en , "chief technology officer"@en ;
	rdfs:comment "The role held by a person who is the chief technology officer of an organization, responsible for directing and managing technical developments for that organization."@en .

:claimant a owl:NamedIndividual , :FinancialRole ;
	rdfs:label "claimant"@en ;
	rdfs:comment "The role held by a person making a financial claim, for example for refund of travel expenses from a project grant."@en .

:co-applicant a owl:NamedIndividual , :ProjectRole ;
	rdfs:label "co-applicant"@en ;
	rdfs:comment "The role a person has of being a co-applicant on the grant application to obtain funding to fund an endeavour such as a research project."@en .

:co-investigator a owl:NamedIndividual , :InvestigationRole ;
	rdfs:label "co-investigator"@en ;
	rdfs:comment "The role a person has of being a co-investigator for a research investigation."@en .

:collaborator a owl:NamedIndividual , :InvestigationRole ;
	rdfs:label "collaborator"@en ;
	rdfs:comment "The role a person, typically from another research group or institution, who collaborates with the team undertaking a research investigation."@en .

:collects-data a owl:NamedIndividual , :ExperimentalContribution ;
	rdfs:label "collects data"@en ;
	rdfs:comment "The contribution of making experimental observations and collecting data, for example using a telescope, a microscope or an analytical instrument."@en .

:college-fellow a owl:NamedIndividual , :OrganizationalRole ;
	rdfs:label "college fellow"@en ;
	rdfs:comment "The role held by an academic as a senior member of a college."@en .

:college-head a owl:NamedIndividual , :OrganizationalRole ;
	rdfs:label "college head"@en ;
	rdfs:comment "The role held by a senior academic who heads a college and acts as its CEO.  Among the many alternative titles give to this role, the most common are President or Master of the college."@en .

:computer-programmer a owl:NamedIndividual , :InvestigationRole ;
	rdfs:label "computer programmer"@en ;
	rdfs:comment "The role a person has of being someone who develops computer software."@en .

:conceives-project a owl:NamedIndividual , :IntellectualContribution ;
	rdfs:label "conceives project"@en ;
	rdfs:comment """The contribution of conceiving the research project in the first place.  

Note that an individual other than the lead applicant and the co-applicants on a grant application may make considerable contributions to conceiving a research project, but may be prevented from being a formal applicant by reason of affiliation or status (e.g. as a non-academic partner, or a research student)."""@en .

:consortium-author a owl:NamedIndividual , :AuthorshipRole ;
	rdfs:label "consortium author"@en ;
	rdfs:comment "The authorship role of an organisation or consortium that has contributed collectively to the work described in the publication and is named in the list of authors, e.g. 'Wellcome Trust Case Control Consortium' or 'Malaria Genomic Epidemiology Network'.  (The members of such a group should be separately listed elsewhere in the document)."@en .

:consultant a owl:NamedIndividual , :InvestigationRole ;
	rdfs:label "consultant"@en ;
	rdfs:comment "The role a person has of being a consultant who is providing expertise or services."@en .

:contact-person a owl:NamedIndividual , :OrganizationalRole ;
	rdfs:label "contact person"@en ;
	rdfs:comment "The role of a person who serves as the primary contact and information provider for third parties wishing to find out more about an organization or an activity."@en .

:contractor a owl:NamedIndividual , :FinancialRole ;
	rdfs:label "contractor"@en ;
	rdfs:comment "The role held by an agent who enters into a contract to undertake specified work or to supply specified services in return for payment."@en .

:controls-project-finances a owl:NamedIndividual , :OrganizationalContribution ;
	rdfs:label "controls project finances"@en ;
	rdfs:comment "The contribution of controlling the financial aspects of the research project, including authorizing expenditure, creating and managing budgets, and submitting financial reports to the funding agency."@en .

:corresponding-author a owl:NamedIndividual , :AuthorshipRole ;
	rdfs:label "corresponding author"@en ;
	rdfs:comment "The role an author has of being the corresponding author of the document, with whom editors and readers should correspond concerning it."@en .

:creates-novel-organisms-or-cells a owl:NamedIndividual , :ExperimentalContribution ;
	rdfs:label "creates novel organisms or cells"@en ;
	rdfs:comment "The contribution of creating novel organisms (e.g. mouse mutants) or cells (e.g. tranformed cell lines), either for use in the research, or as a research output of use to others."@en .

:creates-novel-reagents a owl:NamedIndividual , :ExperimentalContribution ;
	rdfs:label "creates novel reagents"@en ;
	rdfs:comment "The contribution of creating novel physical or chemical compounds, or molecular biology constructs, either for use in the research, or as a research output of use to others."@en .

:creates-software a owl:NamedIndividual , :ExperimentalContribution ;
	rdfs:label "creates software"@en ;
	rdfs:comment "The contribution of creating computer software, including software for experimental control, analytical tools, mathematical model and computational simulations for use in the research, or of creating an IT service (e.g. a bioinformatics database or Web service) of use to others."@en .

:curator a owl:NamedIndividual , :DataRole ;
	rdfs:label "curator"@en ;
	rdfs:comment "The role of an agent that documents, cares for and manages collections of objects (for example in a museum) or of data (as in a data repository or database).  [A data curator is a curator who specializes in digital data, while a librarian is a type of curator specializing in textual publications.]"@en .

:data-creator a owl:NamedIndividual , :DataRole ;
	rdfs:label "data creator"@en ;
	rdfs:comment "The role a person has of being a creator or originator of new data."@en .

:data-curator a owl:NamedIndividual , :DataRole ;
	rdfs:label "data curator"@en ;
	rdfs:comment "The role a person has of being a data curator, responsible for long-term preservation of data, for example those arising from the research project."@en .

:data-manager a owl:NamedIndividual , :DataRole ;
	rdfs:label "data manager"@en ;
