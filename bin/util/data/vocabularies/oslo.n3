	rdfs:subClassOf owl:Thing ;
	dcterms:identifier "oslo:Building" .

:Code a rdfs:Class ;
	rdfs:label "Code"@en ;
	rdfs:comment """<p xmlns=\"http://purl.org/oslo/ns/localgov#\">The Conceptual Model includes a 'Code' datatype that 
      is used for the types of relation and membership properties. This is expressed using the <a xmlns=\"http://purl.org/oslo/ns/localgov#\" href=\"http://www.w3.org/TR/skos-primer/\">SKOS vocabulary</a> with
      a particular 'code' usually being a oslo:Code which is a skos:Concept that is part of a scheme. 
      The intention is that the oslo:Code class be used as follows;</p><ul xmlns=\"http://purl.org/oslo/ns/localgov#\">
        <li xmlns=\"http://purl.org/oslo/ns/localgov#\">for the content property, use skos:notation</li>
        <li xmlns=\"http://purl.org/oslo/ns/localgov#\">the 'list' property will be taken care of by means of the skos:inScheme property;</li>
        <li xmlns=\"http://purl.org/oslo/ns/localgov#\">the list agency property is likely to be applied to the scheme as a whole for which dcterms:creator is appropriate;</li>
        <li xmlns=\"http://purl.org/oslo/ns/localgov#\">the list version property can be fulfilled using schema:version (see <a xmlns=\"http://purl.org/oslo/ns/localgov#\" href=\"http://schema.org/CreativeWork\">http://schema.org/CreativeWork</a>)</li></ul>
    """^^rdf:XMLLiteral ;
	rdfs:isDefinedBy <http://www.w3.org/TR/skos-primer/> ;
	rdfs:subClassOf skos:Concept ;
	dcterms:identifier "oslo:Code" .

person:Person a rdfs:Class ;
	rdfs:label "Person"@en ;
	rdfs:comment """An individual person who may be dead or alive, 
    but not imaginary. It is that restriction that makes person:Person a
    sub class of both foaf:Person and schema:Person which both cover imaginary
    characters as well as real people."""@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/person#> ;
	rdfs:subClassOf foaf:Person , schema:Person ;
	dcterms:identifier "person:Person" .

cpsv:PublicService a owl:Class ;
	rdfs:comment "This class represents the service itself. As noted in the scope (section 1.4), a public service is the capacity to carry out a procedure and exists whether it is used or not. It is a set of deeds and acts performed by or on behalf of a public agency for the benefit of a citizen, a business or another public agency."@en ;
	rdfs:label "Public Service"@en ;
	a rdfs:Class .

cpsv:Input a owl:Class ;
	rdfs:comment "Inputs can by any resource - document, artefact - anything. In a specific context it is likely to be useful to either define a sub class or declare the particular resource to also be of another type as well. A general case might be a foaf:Document but where possible, it is better to refer to a controlled vocabulary of types. dcterms:type should be used to use to provide this information linking to a SKOS Concept."@en ;
	rdfs:label "Input"@en ;
	a rdfs:Class .

cpsv:Output a owl:Class ;
	rdfs:comment "Outputs can by any resource - document, artefact - anything. In a specific context it is likely to be useful to either define a sub class or declare the particular resource to also be of another type as well. A general case might be a foaf:Document but where possible, it is better to refer to a controlled vocabulary of types. dcterms:type should be used to use to provide this information linking to a SKOS Concept."@en ;
	rdfs:label "Output"@en ;
	a rdfs:Class .

:Permission a owl:Class ;
	dcterms:identifier "oslo:Permission" ;
	rdfs:comment "Denotes a the permission of a role. Should be linked with a formal rule."@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:label "Permission"@en ;
	a rdfs:Class .

:PersonRelation a owl:Class ;
	dcterms:identifier "oslo:PersonRelation" ;
	rdfs:comment "A relation between two persons"@en ;
	rdfs:subClassOf :Relation .

:Relation a owl:Class .

:PersonRelation rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:label "PersonRelation"@en ;
	a rdfs:Class .

:Role a owl:Class ;
	dcterms:identifier "oslo:Role" ;
	rdfs:comment "Denotes a role that a Person or other Agent can take in an public service. Instances of this class describe the abstract role; to denote a specific instance of a person playing that role in a specific public service use an instance of `Permission` that is linked with the formal rules `Rule`."@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:label "Role"@en ;
	a rdfs:Class .

:Status a owl:Class ;
	dcterms:identifier "oslo:Status" ;
	rdfs:comment "Status of Public Service"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:label "Status"@en ;
	a rdfs:Class .

cpsv:Rule a owl:Class ;
	rdfs:comment "The Rule class represents the specific rules, guidelines or procedures that the Public Service follows. Instances of the Rule class are FRBR Expressions, that is, a concrete expression, such as a document, of the more abstract concept of the rules themselves."@en ;
	rdfs:label "Rule"@en ;
	rdfs:subClassOf <http://purl.org/vocab/frbr/core#Expression> ;
	a rdfs:Class .

:Relation dcterms:identifier "oslo:Relation" ;
	rdfs:comment "A relation between two agents"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:label "Relation"@en ;
	a rdfs:Class .

:OrganizationRelation a owl:Class ;
	dcterms:identifier "oslo:OrganizationRelation" ;
	rdfs:comment "A relation between two organizations"@en ;
	rdfs:subClassOf :Relation ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:label "OrganizationnRelation"@en ;
	a rdfs:Class .

rov:registration a owl:ObjectProperty ;
	dcterms:identifier "rov:registration" ;
	rdfs:subPropertyOf adms:identifier ;
	rdfs:domain rov:RegisteredOrganization ;
	rdfs:isDefinedBy <http://www.w3.org/ns/regorg#> ;
	rdfs:comment """The legal status of a registered organization is conferred on it by an authority within a given 
      jurisdiction. The registration is therefore a fundamental relationship between a the organization and the authority with 
      which it is registered. The details of the registration are provided as properties of the Identifier class which is 
      defined by ADMS. The vocabulary sets no restriction on the type of legal identifier. In many countries, the business register's 
      identifier is the relevant data point. The tax number often fulfils this function in Spain and elsewhere.
      Although there is no formal cardinality constraint on any property in the Registered Organization Vocabulary, it is 
      questionable whether a description of a registered organization without this property and an associated Identifier class will 
      be of any value."""@en ;
	rdfs:label "registration"@en ;
	a rdf:Property .

rov:companyStatus a owl:ObjectProperty ;
	dcterms:identifier "rov:companyStatus" ;
	rdfs:subPropertyOf org:classification ;
	rdfs:isDefinedBy <http://www.w3.org/TR/vocab-regorg/> ;
	rdfs:comment """Terms like insolvent, bankrupt and in receivership are likely to have 
      different legal implications in different jurisdictions. Best Practice for recording various 
      other status levels is to use the relevant jurisdiction's terms and to do so in a 
      consistent manner using a SKOS Concept Scheme. rov:companyStatus is a sub class of org:classification
      which has a range of skos:Concept."""@en ;
	rdfs:label "company status"@en ;
	a rdf:Property .

cpsv:produces a owl:ObjectProperty ;
	rdfs:range cpsv:Output ;
	rdfs:comment "The produces property links a Public Service to one or more instances of the Output class which is its range."@en ;
	rdfs:label "produces"@en ;
	a rdf:Property .

org:member a owl:ObjectProperty ;
	rdfs:label "member"@en , "membre"@fr ;
	a owl:FunctionalProperty ;
	rdfs:range foaf:Agent ;
	owl:inverseOf org:hasMembership .

org:hasMembership a owl:ObjectProperty .

org:member rdfs:comment "Indicates the Person (or other Agent including Organization) involved in the Membership relationship. Inverse of `org:hasMembership`"@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/org> ;
	rdfs:domain org:Membership ;
	a rdf:Property ;
	rdfs:comment "Indique une personne (ou tout autre Agent, y compris une Organisation) impliqué dans la relation d'Engagement. Inverse de `org:hasMembership`"@fr .

cpsv:hasInput a owl:ObjectProperty ;
	rdfs:range cpsv:Input ;
	rdfs:comment "The hasInput property links a Public Service to one or more instances of the Input class (see below). A specific service may require the presence of certain inputs or combinations of inputs in order to operate. These should be described in an application profile for a given service."@en ;
	rdfs:label "has input"@en ;
	a rdf:Property .

rov:companyActivity a owl:ObjectProperty ;
	dcterms:identifier "rov:companyActivity" ;
	rdfs:subPropertyOf org:classification ;
	rdfs:isDefinedBy <http://www.w3.org/TR/vocab-regorg/> ;
	rdfs:comment """The activity of a company should be 
      recorded using a controlled vocabulary expressed as a SKOS concept scheme. Several such 
      vocabularies exist, many of which map to the UN's ISIC codes. Where a particular controlled vocabulary is in use 
      within a given context, such as SIC codes in the UK, it is acceptable to use these, however, the preferred 
      choice for European interoperability is NACE. rov:companyActivity is a sub class of org:classification which has a range of skos:Concept."""@en ;
	rdfs:label "company activity"@en ;
	a rdf:Property .

cpsv:follows a owl:ObjectProperty ;
	rdfs:range cpsv:Rule ;
	rdfs:comment "The follows property links a service to the Rule(s) under which it operates."@en ;
	rdfs:label "follows"@en ;
	a rdf:Property .

rov:companyType a owl:ObjectProperty ;
	dcterms:identifier "rov:companyType" ;
	rdfs:subPropertyOf org:classification ;
	rdfs:isDefinedBy <http://www.w3.org/TR/vocab-regorg/> ;
	rdfs:comment """This property records the type of company. Familiar types are SA, PLC, LLC, GmbH etc. Each 
      jurisdiction will have a limited set of recognized company types and these should be used in a consistent manner
      using a SKOS Concept Scheme. rov:companyType is a sub class of org:classification which has a range of skos:Concept."""@en ;
	rdfs:label "company type"@en ;
	a rdf:Property .

:extended_address a rdf:Property ;
	dcterms:identifier "locn:address" ;
	rdfs:range :ExtendedAddress ;
	rdfs:label "extended address"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:comment "The oslo:extended_address property relationship associates an <a xmlns=\"http://purl.org/oslo/ns/localgov#\" href=\"#oslo:ExtendedAddress\">Extended Address</a> with a <a xmlns=\"http://purl.org/oslo/ns/localgov#\" href=\"#locn:Address\">Address</a> Class "^^rdf:XMLLiteral ;
	rdfs:domain :BasicAddress .

person:countryOfBirth a rdf:Property ;
	rdfs:label "country of birth"@en ;
	rdfs:comment "The range of country of birth is dcterms:Location and schema:Place."@en ;
	rdfs:range schema:Place , dcterms:Location , locn:Location ;
	rdfs:isDefinedBy <http://www.w3.org/ns/person#> ;
	dcterms:identifier "person:countryOfBirth" .

:validityPeriod a rdf:Property ;
	rdfs:label "validity period"@en ;
	rdfs:comment "Points to a time interval"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:range <http://www.w3.org/2006/time#> ;
	dcterms:identifier "oslo:validityPeriod" .

:parcelId a rdf:Property ;
	dcterms:identifier "oslo:parcelId" ;
	rdfs:range adms:Identifier ;
	rdfs:domain :Parcel ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:comment "The concept of adding a globally unique identifier for each instance of a parcel is a crucial part of the OSLO data spec. The domain of oslo:parcelId is oslo:Parcel."@en ;
	rdfs:label "parcel ID"@en .

dcterms:identifier a rdf:Property ;
	rdfs:label "identifer"@en ;
	rdfs:comment "dcterms:identifier fully represents the Core Vocabulary property of the same name"@en ;
	vann:usageNote "The identifier relation links a resource to any formally issued identifier for that resource other than one that confers legal status upon it. The range is adms:Identifier."@en ;
	rdfs:isDefinedBy <http://dublincore.org/documents/dcmi-terms/> ;
	dcterms:identifier "dcterms:identifier" ;
	vann:usageNote "The identifier relation is used to link a person to any formally issued identifier for that person."@en .

foaf:givenName a rdf:Property ;
	rdfs:label "given name"@en ;
	rdfs:comment "The foaf:givenName property fully represents the Person Core Vocabulary term of given name"@en ;
	vann:usageNote """A given name, or multiple given names, are the denominator(s) that identify an individual 
    within a family. These are given to a person by his or her parents at birth or may be legally recognised 
    for example, the given name for Johan Sebastian Bach is 'Johan Sebastian.'"""@en ;
	rdfs:isDefinedBy <http://xmlns.com/foaf/spec/> ;
	dcterms:identifier "foaf:givenName" .

:establishmentDate a rdf:Property ;
	rdfs:label "date of establishment"@en ;
	rdfs:comment "Date of establishment of an organization"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:domain :Organization ;
	rdfs:range rdfs:Literal ;
	dcterms:identifier "oslo:establishmentDate" .

:country a rdf:Property ;
	rdfs:label "country"@en ;
	rdfs:comment "The uppermost administrative unit for the address, almost always a country. The domain of locn:adminUnitL1 is locn:Address and the range is a literal, conceptually defined by the <a xmlns=\"http://purl.org/oslo/ns/localgov#\" href=\"https://inspire-twg.jrc.ec.europa.eu/inspire-fc.html#_C2612\">INSPIRE Geographical Name data type</a>."^^rdf:XMLLiteral ;
	vann:usageNote "Best practice is to use the ISO 3166-1 code but if this is inappropriate for the context, country names should be provided in a consistent manner to reduce ambiguity. For example, either write 'United Kingdom' or 'UK' consistently throughout the data set and avoid mixing the two."@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/locn#> ;
	rdfs:domain locn:Address ;
	rdfs:subPropertyOf locn:adminUnitL1 ;
	dcterms:identifier "oslo:country" , "urn:x-inspire:specification:gmlas:BaseTypes:3.2:adminUnit" .

:buildingId a rdf:Property ;
	dcterms:identifier "oslo:buldingId" ;
	rdfs:range adms:Identifier ;
	rdfs:domain :Building ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:comment "The concept of adding a globally unique identifier for each instance of a building is a crucial part of the OSLO data spec. The domain of oslo:buildingId is oslo:Building."@en ;
	rdfs:label "building ID"@en .

:nobelityTitle a rdf:Property ;
	rdfs:label "nobelity title"@en ;
	rdfs:comment "Nobelity title of a person"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:domain person:Person ;
	rdfs:range rdfs:Literal ;
	dcterms:identifier "oslo:nobelityTitle" .

:civilClass a rdf:Property ;
	rdfs:label "civil classs"@en ;
	rdfs:comment "The range of place of death is a code."@en ;
	rdfs:domain person:Person ;
	rdfs:range :Code ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	dcterms:identifier "oslo:civilClass" .

:function a rdf:Property ;
	rdfs:label "KBO function"@en ;
	rdfs:comment "A KBO Funtion"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:domain :Organization ;
	rdfs:range :Code ;
	dcterms:identifier "oslo:function" .

:poBox a rdf:Property ;
	rdfs:label "po box"@en ;
	rdfs:comment "The Post Office Box number. The domain of oslo:poBox is oslo:ExtendedAddress."@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/locn#> ;
	rdfs:domain :ExtendedAddress ;
	rdfs:range rdfs:Literal ;
	rdfs:subPropertyOf locn:poBox ;
	dcterms:identifier "oslo:poBox" .

:result a rdf:Property ;
	rdfs:label "Public Service Result"@en ;
	rdfs:comment "Result of the public service process."@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:domain cpsv:PublicService ;
	rdfs:range :Code ;
	dcterms:identifier "oslo:result" .

locn:address a rdf:Property ;
	rdfs:label "address"@en ;
	rdfs:comment "The locn:address property relationship associates any resource with the <a xmlns=\"http://purl.org/oslo/ns/localgov#\" href=\"#locn:Address\">Address</a> Class "^^rdf:XMLLiteral ;
	rdfs:isDefinedBy <http://www.w3.org/ns/locn#> ;
	rdfs:range locn:Address ;
	dcterms:identifier "locn:address" .

:address a rdf:Property ;
	dcterms:identifier "oslo:address" ;
	rdfs:domain :ResidenceObject ;
	rdfs:range :BasicAddress ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:comment "Parcel linked to a building or a residence object."@en ;
	rdfs:label "addres"@en .

foaf:name a rdf:Property ;
	rdfs:label "name"@en ;
	rdfs:comment "The foaf:name property fully represents the Person Core Vocabulary term of full name"@en ;
	vann:usageNote """The full name contains the complete name of a person 
    as one string. In addition to the content of given name, family name and, in some 
    systems, patronymic name, this can carry additional parts of a person's name 
    such as titles, middle names or suffixes like \"the third\" or names which are neither 
    a given nor a family name."""@en ;
	rdfs:isDefinedBy <http://xmlns.com/foaf/spec/> ;
	dcterms:identifier "foaf:name" .

:fromPerson a rdf:Property ;
	rdfs:range :Person ;
	rdfs:domain :PersonRelation ;
	rdfs:comment "from relation"@en ;
	rdfs:label "from"@en .

:companyStatus a rdf:Property ;
	rdfs:label "KBO Company status"@en ;
	rdfs:comment "A KBO Company status"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:domain :Organization ;
	rdfs:range :Code ;
	dcterms:identifier "oslo:companyStatus" .

:providesRole a rdf:Property ;
	rdfs:range :Role ;
	rdfs:domain cpsv:PublicService ;
	rdfs:comment "This very general property links an Public Service to a Role."@en ;
	rdfs:label "provides role"@en .

:productType a rdf:Property ;
	rdfs:label "type of product"@en ;
	rdfs:comment "Type of a product"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:domain :Product ;
	rdfs:range :Code ;
	rdfs:subPropertyOf <http://purl.org/dc/elements/1.1/type> ;
	dcterms:identifier "oslo:productType" .

schema:gender a rdf:Property ;
	rdfs:label "gender"@en ;
	rdfs:comment "The schema:gender property fully represents the Person Core Vocabulary term of the same name."@en ;
	vann:usageNote """The gender of an individual should be recorded using a controlled vocabulary 
    that is appropriate for the specific context. In some cases the chromosomal or physical state of an 
    individual will be more important than the gender that they express, in others the reverse will be true. 
    What is always important is that the controlled vocabulary used to describe an individual's gender 
    is stated explicitly. To do this use a skos:Concept as described in the <a xmlns=\"http://purl.org/oslo/ns/localgov#\" href=\"#skos:Concept\">Code</a> class.
    The <a xmlns=\"http://purl.org/oslo/ns/localgov#\" href=\"https://joinup.ec.europa.eu/system/files/project/Core_Vocabularies-Business_Location_Person-Specification-v0.3_0.zip\">Person Core Vocabulary</a> includes an example of such a SKOS concept scheme."""^^rdf:XMLLiteral ;
	rdfs:isDefinedBy <http://schema.org/docs/schemaorg.owl> ;
	dcterms:identifier "schema:gender" .

:membershipFunction a rdf:Property ;
	rdfs:label "membership function"@en ;
	rdfs:comment "Function label of a membership"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:domain :Membership ;
	rdfs:range rdfs:Literal ;
	dcterms:identifier "oslo:membershipFunction" .

person:birthName a rdf:Property ;
	rdfs:label "birth name"@en ;
	rdfs:comment """All data associated with an individual are subject to change. Names can change for 
    a variety of reasons, either formally or informally, and new information may come to light that means that 
    a correction or clarification can be made to an existing record. Birth names tend to be persistent 
    however and for this reason they are recorded by some public sector information systems. There is 
    no granularity for birth name - the full name should be recorded in a single field."""@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/person#> ;
	dcterms:identifier "person:birthName" .

:statusCode a rdf:Property ;
	rdfs:label "Statuscode"@en ;
	rdfs:comment "Statuscode"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:domain :Status ;
	rdfs:range :Code ;
	rdfs:subPropertyOf <http://purl.org/dc/elements/1.1/type> ;
	dcterms:identifier "oslo:statusCode" .

legal:legalIdentifier a rdf:Property ;
	rdfs:label "legal identifer"@en ;
	rdfs:comment """The legal identifier is a fundamental relationship between a legal entity and the 
    authority with which it is registered and that confers legal status upon it. legal:legalIdentifier is 
    a sub property of dcterms:identifier. legal:legalIdentifier has a
    defined domain of legal:LegalEntity and a range of adms:Identifier"""@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/legal#> ;
	rdfs:domain legal:LegalEntity ;
	rdfs:range adms:Identifier ;
	rdfs:subPropertyOf dcterms:identifier ;
	dcterms:identifier "legal:legalIdentifier" .

:residence a rdf:Property ;
	rdfs:label "residence"@en ;
	rdfs:comment "The range of residence is ResidenceObject."@en ;
	rdfs:range :ResidenceObject ;
	rdfs:domain person:Person ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	dcterms:identifier "oslo:residence" .

legal:companyActivity a rdf:Property ;
	rdfs:label "company activity"@en ;
	rdfs:comment """The activity of a company should be recorded using a controlled vocabulary expressed
    as a SKOS concept scheme (see <a xmlns=\"http://purl.org/oslo/ns/localgov#\" href=\"#skos:Concept\">Code</a>. Several such 
    vocabularies exist, many of which map to the UN's ISIC codes. Where a particular controlled vocabulary is in use 
    within a given context, such as SIC codes in the UK, it is acceptable to use these, however, the preferred 
    choice for European interoperability is NACE."""^^rdf:XMLLiteral ;
	rdfs:isDefinedBy <http://www.w3.org/ns/legal#> ;
	rdfs:subPropertyOf org:classification ;
	rdfs:range skos:Concept ;
	dcterms:identifier "legal:companyActivity" .

person:citizenship a rdf:Property ;
	rdfs:label "citizenship"@en ;
	rdfs:comment "The range of citizenship is dcterms:Jurisdiction."@en ;
	rdfs:range dcterms:Jurisdiction ;
	rdfs:isDefinedBy <http://www.w3.org/ns/person#> ;
	dcterms:identifier "person:citizenship" .

rdfs:seeAlso a rdf:Property ;
	rdfs:label "geographic identifier"@en ;
	rdfs:comment "rdfs:seeAlso fully represents the Location Core Vocabulary concept of a geographic identifier."@en ;
	vann:usageNote """Used in the Location Core vocabulary to provide a URI that identifies the location. 
    This should be expressed using the rdfs:seeAlso property unless the identifier is already the subject of the description. 
    Examples include URIs from <a xmlns=\"http://purl.org/oslo/ns/localgov#\" href=\"http://www.geonames.org/\">GeoNames.org</a> and <a xmlns=\"http://purl.org/oslo/ns/localgov#\" href=\"http://dbpedia.org/\">DBpedia</a> 
    such as http://dbpedia.org/resource/ISO_3166-2:XX where XX is the ISO 3166 two character code for a country."""^^rdf:XMLLiteral ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	dcterms:identifier "rdfs:seeAlso" .

:legalFormCode a rdf:Property ;
	rdfs:label "legal form code"@en ;
	rdfs:comment "A Legal Form Code"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:domain :Organization ;
	rdfs:range :Code ;
	rdfs:subPropertyOf rov:companyType ;
	dcterms:identifier "oslo:legalFormCode" .

:hasActor a rdf:Property ;
	olca:rangeIncludes :Membership , :Organization , person:Person ;
	rdfs:domain :Role ;
	rdfs:comment "This very general property links an Public Service to a Role."@en ;
	rdfs:label "provides role"@en .

legal:companyType a rdf:Property ;
	rdfs:label "company type"@en ;
	rdfs:comment """This property records the type of company. Familiar types are SA, PLC, LLC, GmbH etc. Each 
    jurisdiction will have a limited set of recognised company types and these should be used in a consistent manner
    using a oslo:Code as described in the <a xmlns=\"http://purl.org/oslo/ns/localgov#\" href=\"#oslo:Code\">Code</a> Class."""^^rdf:XMLLiteral ;
	rdfs:isDefinedBy <http://www.w3.org/ns/legal#> ;
	rdfs:range :Code ;
	rdfs:subPropertyOf org:classification ;
	dcterms:identifier "legal:companyType" .

:isBranch a rdf:Property ;
	rdfs:label "organization is a branch or not"@en ;
	rdfs:comment "Depicts if organization is branch of another organization"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:domain :Organization ;
	rdfs:range rdfs:Literal ;
	dcterms:identifier "oslo:isBranch" .

:locatorName a rdf:Property ;
	rdfs:label "locator name"@en ;
	rdfs:comment "Proper noun(s) applied to the real world entity identified by the locator. The locator name could be the name of the property or complex, of the building or part of the building, or it could be the name of a room inside a building. The domain of oslo:locatorName is oslo:ExtendedAddress."@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:domain :ExtendedAddress ;
	rdfs:range rdfs:Literal ;
	rdfs:subPropertyOf <http://www.w3.org/ns/locn#locn;locatorName> ;
	dcterms:identifier "oslo:locatorName" .

:functionPhase a rdf:Property ;
	rdfs:label "KBO function phase"@en ;
	rdfs:comment "A KBO Funtionphase"@en ;
	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:domain :Organization ;
	rdfs:range :Code ;
	dcterms:identifier "oslo:functionPhase" .

legal:legalEntity a rdf:Property ;
	rdfs:label "legal entity"@en ;
	rdfs:comment "The legal entity relationship can be used to link any resource to a Legal Entity Class. This is useful, for example, where an organisation includes one or more legal entities. The Dublin Core term isPartOf is a suitable inverse of this relationship. legal:legalEntity has a range of legal:LegalEntity."@en ;
	rdfs:isDefinedBy <http://www.w3.org/ns/legal#> ;
	rdfs:range legal:LegalEntity ;
	dcterms:identifier "legal:legalEntity" .

:legalIncompetence a rdf:Property ;
	rdfs:label "legal incompetence"@en ;
