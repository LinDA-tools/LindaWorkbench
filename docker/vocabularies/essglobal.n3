	rdfs:range <http://purl.org/essglobal/vocab/v1.0/Cost> .

<http://purl.org/essglobal/vocab/v1.0/isComposedBy> a owl:ObjectProperty ;
	rdfs:label "Is Composed By"@en , "É Composto Por"@pt ;
	dc:description "A breakdown of all inputs (as taxes and raw materials) and labour costs that make up the product or service's final cost. An open cost is a pre-requisite for solidarity fair trade systems."@en , "Discriminação de todos os insumos (como impostos e matérias-primas) e custos de mão de obra que compõem o custo final do produto ou serviço. Um preço transparente é um pré-requisito para sistemas solidários de comércio justo."@pt ;
	rdfs:isDefinedBy <http://purl.org/essglobal/vocab/v1.0/> ;
	dc:issued "2014-02-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	dc:modified "2014-08-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	rdfs:domain <http://purl.org/essglobal/vocab/v1.0/Cost> ;
	rdfs:range <http://purl.org/essglobal/vocab/v1.0/CostComposition> .

<http://purl.org/essglobal/vocab/v1.0/hasInputCost> a owl:ObjectProperty ;
	rdfs:label "Has input"@en , "Tem insumo"@pt ;
	dc:description "A product, service, or activity that goes into making the final product or service."@en , "Um produto, serviço ou atividade usado para fazer o produto final ou o serviço."@pt ;
	rdfs:isDefinedBy <http://purl.org/essglobal/vocab/v1.0/> ;
	dc:issued "2014-02-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	dc:modified "2014-08-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	rdfs:domain <http://purl.org/essglobal/vocab/v1.0/CostComposition> ;
	rdfs:range <http://purl.org/essglobal/vocab/v1.0/Input> .

<http://purl.org/essglobal/vocab/v1.0/hasLabourCost> a owl:ObjectProperty ;
	rdfs:label "Has Labour"@en , "Tem Trabalho"@pt ;
	dc:description "Work done for specific tasks related to the provision of goods or services offered by the SSE initiative. Generally it can be human, animal or machine labour, but in ESSglobal it's human labour only."@en , "Trabalho realizado para tarefas específicas relacionadas com o fornecimento de bens ou serviços oferecidos pela iniciativa da ESS. Em geral pode ser humano, animal ou trabalho de máquinas, mas em ESS global é apenas o trabalho humano."@pt ;
	rdfs:isDefinedBy <http://purl.org/essglobal/vocab/v1.0/> ;
	dc:issued "2014-02-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	dc:modified "2014-08-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	rdfs:domain <http://purl.org/essglobal/vocab/v1.0/CostComposition> ;
	rdfs:range <http://purl.org/essglobal/vocab/v1.0/Labour> .

<http://purl.org/essglobal/vocab/v1.0/hasOtherCosts> a owl:ObjectProperty ;
	rdfs:label "Has Other Costs"@en , "Tem Outros Custos"@pt ;
	dc:description "Other costs which impact on the final cost of a product or service provided by an SSE initiative, other than input or labour, like taxes, deprecation of machinery, funds, etc. The delivery costs are not included in these costs: they are included in the sale-options class."@en , "Outros custos que têm impacto no custo final de um produto ou serviço fornecido por uma iniciativa da ESS, com excepção de insumo ou mão de obra, como impostos,desaprovação de maquinaria, fundos, etc. Os custos de entrega não estão incluídos nestes custos: eles estão incluídos na categoria opções-venda."@pt ;
	rdfs:isDefinedBy <http://purl.org/essglobal/vocab/v1.0/> ;
	dc:issued "2014-02-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	dc:modified "2014-08-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	rdfs:domain <http://purl.org/essglobal/vocab/v1.0/CostComposition> ;
	rdfs:range <http://purl.org/essglobal/vocab/v1.0/OtherCosts> .

<http://purl.org/essglobal/vocab/v1.0/availableAt> a owl:ObjectProperty ;
	rdfs:label "Available At"@en , "Disponível Em"@pt ;
	dc:description "A Product or Service is sold at a sale location under specific definitions."@en , "Um Produto ou Serviço é vendido em um local de venda ao abrigo de definições especificas."@pt ;
	rdfs:isDefinedBy <http://purl.org/essglobal/vocab/v1.0/> ;
