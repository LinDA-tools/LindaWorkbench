<http://purl.org/NET/scovo#Dataset> a rdfs:Class , owl:Class ;
	rdfs:label "Dataset" ;
	rdfs:comment "a statistical dataset" .

<http://purl.org/NET/scovo#Item> a rdfs:Class , owl:Class ;
	rdfs:label "Item" ;
	rdfs:comment "a statistical data item" .

<http://purl.org/NET/scovo#Dimension> a rdfs:Class , owl:Class ;
	rdfs:label "Dimension" ;
	rdfs:comment "a dimension of a statistical data item" .

<http://purl.org/NET/scovo#dataset> a rdf:Property ;
	rdfs:label "belongs to dataset" ;
	rdfs:domain <http://purl.org/NET/scovo#Item> ;
	rdfs:range <http://purl.org/NET/scovo#Dataset> .

<http://purl.org/NET/scovo#datasetOf> a rdf:Property ;
	rdfs:label "is the dataset of" ;
	rdfs:domain <http://purl.org/NET/scovo#Dataset> ;
	rdfs:range <http://purl.org/NET/scovo#Item> .

<http://purl.org/NET/scovo#dimension> a rdf:Property ;
	rdfs:label "has a dimension" ;
	rdfs:domain <http://purl.org/NET/scovo#Item> ;
	rdfs:range <http://purl.org/NET/scovo#Dimension> .

<http://purl.org/NET/scovo#min> a rdf:Property ;
	rdfs:label "has a minimum range value" ;
	rdfs:domain <http://purl.org/NET/scovo#Dimension> .

<http://purl.org/NET/scovo#max> a rdf:Property ;
	rdfs:label "has a maximum range value" ;
	rdfs:domain <http://purl.org/NET/scovo#Dimension> .
