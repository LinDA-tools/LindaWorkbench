	reg:name "hasAttributedWorksFrom"@en ;
	rdfs:label "has attributed works from"@en ;
	skos:definition "Relates a person (either a \"real\" individual or a persona) or identity to whom one or more works by another person have been attributed, either erroneously or falsely, and that other person."@en ;
	a :Property ;
	rdfs:domain <http://iflastandards.info/ns/fr/frbr/frbrer/C1005> ;
	rdfs:range <http://iflastandards.info/ns/fr/frbr/frbrer/C1005> ;
	owl:inverseOf <http://iflastandards.info/ns/fr/frad/P4043> .

<http://iflastandards.info/ns/fr/frad/P3017> rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frad/> ;
	reg:status status:1001 ;
