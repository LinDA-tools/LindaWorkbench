:isElementOf a owl:ObjectProperty ;
	rdfs:label "is element of"@en ;
	rdfs:comment "The link between an entity and the collection containing it."@en ;
	rdfs:range collections:Collection ;
	owl:inverseOf collections:element ;
	rdfs:subPropertyOf owl:topObjectProperty .

:isReferencedBy a owl:ObjectProperty ;
	rdfs:label "is referenced by"@en ;
	rdfs:comment "The relation between a publication and the bibliographic record or bibliographic reference describing it."@en ;
	rdfs:domain core:Endeavour ;
	rdfs:subPropertyOf owl:topObjectProperty ;
	rdfs:range _:node17168lpk7x19 .

_:node17168lpk7x19 a owl:Class ;
	owl:unionOf _:node17168lpk7x20 .

_:node17168lpk7x20 rdf:first :BibliographicRecord ;
	rdf:rest _:node17168lpk7x21 .

_:node17168lpk7x21 rdf:first :BibliographicReference ;
	rdf:rest rdf:nil .

:references a owl:ObjectProperty ;
	rdfs:label "references"@en ;
	rdfs:comment "The relation between a bibliographic record or a bibliographic reference and the publication being referenced."@en ;
	owl:inverseOf :isReferencedBy ;
	rdfs:range core:Endeavour ;
	rdfs:subPropertyOf owl:topObjectProperty ;
	rdfs:domain _:node17168lpk7x22 .

_:node17168lpk7x22 a owl:Class ;
	owl:unionOf _:node17168lpk7x23 .

_:node17168lpk7x23 rdf:first :BibliographicRecord ;
	rdf:rest _:node17168lpk7x24 .

_:node17168lpk7x24 rdf:first :BibliographicReference ;
	rdf:rest rdf:nil .

core:part a owl:ObjectProperty .

core:realization a owl:ObjectProperty .

