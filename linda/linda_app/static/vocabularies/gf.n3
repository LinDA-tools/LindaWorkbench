        dc:title                       "OWL representation of ISO 19109 (General Feature Model)"^^xsd:string ;
        dc:type                        "http://www.w3.org/2002/07/owl"^^rdf:PlainLiteral ;
        dct:created                    "2010-10-27T00:00:00"^^xsd:dateTime ;
