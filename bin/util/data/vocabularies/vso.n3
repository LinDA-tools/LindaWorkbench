@prefix dc:    <http://purl.org/dc/elements/1.1/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix wikipedia: <http://en.wikipedia.org/wiki/> .
@prefix vso:   <http://purl.org/vso/ns#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix gr:    <http://purl.org/goodrelations/v1#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

vso:fuelType  a             owl:ObjectProperty ;
        rdfs:comment        "The type of fuel suitable for the engine or engines of the vehicle.\n\nUse DBPedia resources to indicate the fuel type. Popular identifiers are\n  http://dbpedia.org/resource/Gasoline\n  http://dbpedia.org/resource/Diesel\n  http://dbpedia.org/resource/Biodiesel\n  http://dbpedia.org/resource/Kerosene\n\nFor two-stroke mixtures, use vso:TwoStrokeMixture. "@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "fuelType (0..*)"@en ;
        rdfs:range          vso:FuelTypeValue ;
        rdfs:subPropertyOf  gr:qualitativeProductOrServiceProperty .

vso:AWD  a                vso:DriveWheelConfigurationValue ;
        rdfs:comment      "All-wheel Drive is a transmission layout where the engine drives all four wheels."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "All-wheel drive (vso:DriveWheelConfigurationValue)"@en ;
        rdfs:seeAlso      wikipedia:Four-wheel_drive .

vso:Watercraft  a         owl:Class ;
        rdfs:comment      "A watercraft is a vehicle, vessel or craft designed to move across or through water."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Watercraft (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Watercraft , wikipedia:Watercraft ;
        rdfs:subClassOf   vso:Vehicle .

vso:BusOrCoach  a         owl:Class ;
        rdfs:comment      "A bus (also omnibus or autobus) is a road vehicle designed to carry passengers. Buses have a capacity as high as 300 passengers and are widely used for public transportation. \nCoaches are luxury busses, usually in service for long distance travel."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Bus or coach (gr:ProductOrService)"@en ;
        rdfs:seeAlso      <http://dbpedia.org/page/Coach_%28vehicle%29> , dbpedia:Bus , <http://en.wikipedia.org/wiki/Coach_%28vehicle%29> , wikipedia:Bus ;
        rdfs:subClassOf   vso:MotorizedRoadVehicle .

vso:SteeringPositionValue
        a                 owl:Class ;
        rdfs:comment      "A value indicating a steering position"@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Steering position value (the class of predefined values)"@en ;
        rdfs:subClassOf   gr:QualitativeValue .

vso:EmissionStandardValue
        a                 owl:Class ;
        rdfs:comment      "A value indicating the an emission standard.\n\nYou can safely use any reasonable DBPedia URI, e.g. \n  http://dbpedia.org/resource/Super_Ultra_Low_Emission_Vehicle\n  http://dbpedia.org/resource/Partial_zero-emissions_vehicle\n\nSee also http://en.wikipedia.org/wiki/Emissions_standard.\n\nUnfortunately, there are no DBPedia identifiers for the popular Euro 1 - Euro 5 standards available.\nIf you cannot find a suitable identifier in DBPedia, define your own as an instance of vso:EmissionStandardValue."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Emission standard value (the class of predefined values)"@en ;
        rdfs:subClassOf   gr:QualitativeValue .

vso:Vehicle  a            owl:Class ;
        rdfs:comment      "A vehicle is a device that is designed or used to transport people or cargo over land, water, air, or through space."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Vehicle (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Vehicle , wikipedia:Vehicle ;
        rdfs:subClassOf   gr:ProductOrService .

dcterms:license  a  owl:AnnotationProperty .

vso:FuelQuantity  a       owl:Class ;
        rdfs:comment      "A value indicating an amount of fuel"@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "FuelQuantity"@en ;
        rdfs:subClassOf   gr:QuantitativeValueFloat .

dc:subject  a   owl:AnnotationProperty .

vso:driveWheelConfiguration
        a                   owl:ObjectProperty ;
        rdfs:comment        "The drive wheel configuration, i.e. which roadwheels will receive torque from the vehicle's engine via the drivetrain"@en ;
        rdfs:domain         vso:MotorizedRoadVehicle ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "driveWheelConfiguration (0..1)"@en ;
        rdfs:range          vso:DriveWheelConfigurationValue ;
        rdfs:seeAlso        wikipedia:Drive_wheel ;
        rdfs:subPropertyOf  gr:qualitativeProductOrServiceProperty .

vso:BodyStyleValue  a     owl:Class ;
        rdfs:comment      "A value indicating the body style of a vehicle.\n\nYou can safely use any reasonable DBPedia URI, e.g. \n  http://dbpedia.org/resource/Convertible\n  http://dbpedia.org/resource/Hatchback\n  http://dbpedia.org/resource/Station_wagon\n  http://dbpedia.org/resource/Sport_utility_vehicle\n  http://dbpedia.org/resource/Roadster"@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Body style value (the class of predefined values)"@en ;
        rdfs:subClassOf   gr:QualitativeValue .

vso:FeatureValue  a       owl:Class ;
        rdfs:comment      "A value indicating the features or accessories that are attached to or belong to a vehicle.\n\nYou can safely use any reasonable DBPedia URI, e.g. \n  http://dbpedia.org/resource/Anti-lock_braking_system\n  http://dbpedia.org/resource/Electronic_stability_control\n  http://dbpedia.org/resource/Armrest\n  http://dbpedia.org/resource/Spoiler_%28automotive%29\n  http://dbpedia.org/resource/CD_Changer\n  http://dbpedia.org/resource/Cup_holder\n  http://dbpedia.org/resource/Tow_hitch\n  http://dbpedia.org/resource/Power_window\n  http://dbpedia.org/resource/Power_steering\n  http://dbpedia.org/resource/Air_conditioner\n  http://dbpedia.org/resource/Child_safety_lock\n  http://dbpedia.org/resource/Parking_sensors"@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Feature value (the class of predefined values)"@en ;
        rdfs:subClassOf   gr:QualitativeValue .

vso:damages  a              owl:DatatypeProperty ;
        rdfs:comment        "A textual description of known damages, both repaired and unrepaired.\n\nNote: This property should be used with gr:ActualProductOrServiceInstance (when offering a vehicle) or gr:ProductOrServicesSomeInstancesPlaceholder (when seeking a vehicle using gr:seeks) only."@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "damages (0..*)"@en ;
        rdfs:range          rdfs:Literal ;
        rdfs:subPropertyOf  gr:datatypeProductOrServiceProperty .

vso:color  a                owl:DatatypeProperty ;
        rdfs:comment        "The color of the vehicle"@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "color (0..*)"@en ;
        rdfs:range          rdfs:Literal ;
        rdfs:subPropertyOf  gr:datatypeProductOrServiceProperty .

vso:TwoStrokeMixture  a   vso:FuelTypeValue ;
        rdfs:comment      "A pre-mixed fuel-oil mixture"@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Two-Stroke Mixture (vso:FuelTypeValue)"@en .

vso:productionDate  a       owl:DatatypeProperty ;
        rdfs:comment        "The date of production of the vehicle. This property should be used mainly with gr:ActualProductOrServiceInstance."@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "productionDate (0..1)"@en ;
        rdfs:range          xsd:date ;
        rdfs:subPropertyOf  gr:datatypeProductOrServiceProperty .

vso:engineDisplacement
        a                   owl:ObjectProperty ;
        rdfs:comment        "The volume swept by all of the pistons inside the cylinders of an internal combustion engine in a single movement. If there are multiple engines, simply attach this property multiple times.\nTypical unit code(s): CMQ for cubic centimeter, LTR for liters, INQ for cubic inches\n\t\nNote 1: You can link to information about how the given value has been determined using the gr:valueReference property.\nNote 2: You can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges."@en ;
        rdfs:domain         [ a            owl:Class ;
                              owl:unionOf  ( vso:MotorizedRoadVehicle vso:Boat vso:Ship )
                            ] ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "engineDisplacement (0..*)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:seeAlso        wikipedia:Engine_displacement ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:MotorBoat  a          owl:Class ;
        rdfs:comment      "A motorboat is a boat which is powered by an engine."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Motor boat (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Motorboat , wikipedia:Motorboat ;
        rdfs:subClassOf   vso:Boat .

vso:MotorizedRoadVehicle
        a                 owl:Class ;
        rdfs:comment      "A motorized road vehicle is a wheeled land vehicle whose main propulsion is provided by an engine or motor."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Motorized road vehicle (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Motor_vehicle , wikipedia:Motor_vehicle ;
        rdfs:subClassOf   vso:Vehicle .

vso:previousOwners  a       owl:ObjectProperty ;
        rdfs:comment        "The number of previous owners of the vehicle, including the current one\nTypical unit code(s): C62\n\t\nNote: This property should be used with gr:ActualProductOrServiceInstance (when offering a vehicle) or gr:ProductOrServicesSomeInstancesPlaceholder (when seeking a vehicle using gr:seeks) only."@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "previousOwners (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueInteger ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:condition  a            owl:DatatypeProperty ;
        rdfs:comment        "A textual description of the condition of the vehicle. This property should be mainly used with gr:ActualProductOrServiceInstance."@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "condition (0..*)"@en ;
        rdfs:range          rdfs:Literal ;
        rdfs:subPropertyOf  gr:datatypeProductOrServiceProperty .

vso:Quadracycle  a        owl:Class ;
        rdfs:comment      "A quadracycle is a four-wheeled human-powered vehicle. It is also referred to as a quadricycle, quadcycle pedal car or four-wheeled bicycle."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Quadracycle (gr:ProductOrService) "@en ;
        rdfs:seeAlso      dbpedia:Quadracycle , wikipedia:Quadracycle ;
        rdfs:subClassOf   vso:Vehicle .

vso:Truck  a              owl:Class ;
        rdfs:comment      "A lorry (British English) or truck (American English) is a motor vehicle designed to transport cargo."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Truck (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Truck , wikipedia:Truck ;
        rdfs:subClassOf   vso:MotorizedRoadVehicle .

vso:modelDate  a            owl:DatatypeProperty ;
        rdfs:comment        "The release date of a vehicle model (often used to differentiate versions of the same make and model)"@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "modelDate (0..1)"@en ;
        rdfs:range          xsd:date ;
        rdfs:subPropertyOf  gr:datatypeProductOrServiceProperty .

vso:transmission  a         owl:ObjectProperty ;
        rdfs:comment        "The type of component used for transmitting the power from a rotating power source to the wheels or propeller(s) (\"gearbox\" for cars).\n\nUse DBPedia resources to indicate the transmission type, if possible. Popular identifiers are\n  http://dbpedia.org/resource/Manual_transmission\n  http://dbpedia.org/resource/Automatic_transmission\n\nIf you cannot find a suitable identifier in DBPedia, define your own as an instance of vso:TransmissionTypeValue."@en ;
        rdfs:domain         [ a            owl:Class ;
                              owl:unionOf  ( vso:MotorizedRoadVehicle vso:Boat vso:Ship vso:Bicycle vso:Quadracycle )
                            ] ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "transmission (0..*)"@en ;
        rdfs:range          vso:TransmissionTypeValue ;
        rdfs:seeAlso        <http://dbpedia.org/page/Transmission_%28mechanics%29> , <http://en.wikipedia.org/wiki/Transmission_%28mechanics%29> ;
        rdfs:subPropertyOf  gr:qualitativeProductOrServiceProperty .

vso:steeringPosition  a     owl:ObjectProperty ;
        rdfs:comment        "The position of the steering wheel or similar device (mostly for cars)"@en ;
        rdfs:domain         vso:MotorizedRoadVehicle ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "steeringPosition (0..1)"@en ;
        rdfs:range          vso:SteeringPositionValue ;
        rdfs:subPropertyOf  gr:qualitativeProductOrServiceProperty .

vso:Van  a                owl:Class ;
        rdfs:comment      "A van is a kind of vehicle used for transporting  goods or groups of people. It is usually a box-shaped vehicle on four wheels, about the same width and length as a large automobile, \nbut taller and usually higher off the ground, also referred to as a light commercial vehicle or LCV."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Van (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Van , wikipedia:Van ;
        rdfs:subClassOf   vso:MotorizedRoadVehicle .

vso:speed  a                owl:ObjectProperty ;
        rdfs:comment        "The speed range of the vehicle. If the vehicle is powered by an engine, the upper limit of the speed range (indicated by gr:hasMaxValueFloat) should be the maximum speed achievable under regular conditions.\nTypical unit code(s): KMH for km/h, HM for mile per hour (0.447 04 m/s), KNT for knot\n\t\nNote 1: Use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate the range. Typically, the minimal value is zero.\nNote 2: There are many different ways of measuring the speed range. You can link to information about how the given value has been determined using the gr:valueReference property."@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "speed (0..*)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:0to100KMH  a                 vso:SpeedInterval ;
        rdfs:comment             "The speed interval from 0 to 100 km/h"@en ;
        rdfs:isDefinedBy         <http://purl.org/vso/ns> ;
        rdfs:label               "0to100KMH (vso:SpeedInterval)"@en ;
        gr:hasMaxValueFloat      "100.0"^^xsd:float ;
        gr:hasMinValueFloat      "0.0"^^xsd:float ;
        gr:hasUnitOfMeasurement  "KMH"^^xsd:string .

dc:creator  a   owl:AnnotationProperty .

<http://purl.org/vso/ns>
        a                owl:Ontology ;
        rdfs:comment     "VSO: The Vehicle Sales Ontology\n    \nA Web vocabulary for describings cars, boats, bikes, and other vehicles for e-commerce.\n\nVocabulary:    http://purl.org/vso/ns\nNamespace:     http://purl.org/vso/ns#\n\nThe Vehicle Sales Ontology is designed to be used in combination with GoodRelations, a standard vocabulary for the commercial aspects of offers for sale or rental. \n\nSee http://purl.org/goodrelations/ for more information."@en ;
        rdfs:label       "Vehicle Sales Ontology"@en ;
        rdfs:seeAlso     <http://purl.org/goodrelations/> ;
        dc:contributor   "Many class and property definitions are inspired by or based on abstracts from Wikipedia, the free encyclopedia."@en ;
        dc:creator       "Martin Hepp"@en ;
        dc:rights        "The Vehicle Sales Ontology ontology is available under the Creative Commons Attribution 3.0 Unported license; see http://creativecommons.org/licenses/by/3.0/. \nIn a nutshell, you are free to copy, distribute and transmit the work; to remix/adapt the work (e.g. to import the ontology and create specializations of its elements), \nas long as you attribute the work in the manner specified by the author or licensor (but not in any way that suggests that they endorse you or your use of the work)."@en ;
        dc:subject       "E-Commerce, E-Business, GoodRelations, Ontology, Cars, Automobiles, Boats, Bikes"@en ;
        dc:title         "VSO: The Vehicle Sales Ontology for Semantic Web-based E-Commerce"@en ;
        dcterms:license  <http://creativecommons.org/licenses/by/3.0/> ;
        owl:imports      <http://purl.org/goodrelations/v1> ;
        owl:versionInfo  "V 1.0, Release 2010-10-02" .

vso:RWD  a                vso:DriveWheelConfigurationValue ;
        rdfs:comment      "Rear-wheel drive is a transmission layout where the engine drives the rear wheels."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Rear-wheel drive, RWD (vso:DriveWheelConfigurationValue)"@en ;
        rdfs:seeAlso      <http://en.wikipedia.org/wiki/Automobile_layout#Rear_wheel_drive_layouts> , <http://en.wikipedia.org/wiki/Drive_wheel#Rear-wheel_drive> .

vso:Automobile  a         owl:Class ;
        rdfs:comment      "An automobile, motor car, or car is a wheeled motor vehicle used for transporting passengers, which also carries its own engine or motor."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Automobile (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Automobile , wikipedia:Automobile ;
        rdfs:subClassOf   vso:MotorizedRoadVehicle .

vso:referenceFuelQuantity
        a                   owl:ObjectProperty ;
        rdfs:comment        "The quantity of fuel to which a vso:fuelEconomy value refers"@en ;
        rdfs:domain         gr:QuantitativeValueFloat ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "referenceFuelQuantity (0..1)"@en ;
        rdfs:range          vso:FuelQuantity ;
        rdfs:subPropertyOf  gr:valueReference .

vso:fuelConsumption  a      owl:ObjectProperty ;
        rdfs:comment        "The amount of fuel consumed for traveling a particular distance or temporal duration with the given vehicle (e.g. liters per 100 km)\nTypical unit code(s): LTR for liters, GLL of US Gallons, GLI for UK / Imperial Gallons\n\t\t\nNote 1: There are unfortunately no standard unit codes for \"liters per 100 km\". Simply use \"LTR\" for liters, \"GLL\" of US Gallons, or \"GLI\" for UK / Imperial Gallons, and indicate the distance in the rdfs:label of the gr:QuantitativeValueFloat or use vso:referenceDistance to model the reference distance (e.g. 100 km). \nNote 2: There are two ways of indicating the fuel consumption, vso:fuelConsumption (e.g. 8 liters per 100 km) and vso:fuelEconomy (e.g. 30 miles per gallon). They are reciprocal.\nNote 3: Often, the absolute value is useful only when related to driving speed (\"at 80 km/h\") or usage pattern (\"city traffic\"). You can use gr:valueReference to link the value for the fuel consumption to another value."@en ;
        rdfs:domain         [ a            owl:Class ;
                              owl:unionOf  ( vso:MotorizedRoadVehicle vso:Boat vso:Ship )
                            ] ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "fuelConsumption (0..*)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:seeAlso        wikipedia:Fuel_economy_in_automobiles ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:meetsEmissionStandard
        a                   owl:ObjectProperty ;
        rdfs:comment        "Indicates that the vehicle meets the respective emission standard. \n\nUse DBPedia resources to indicate the standard, if possible. Popular identifiers are\n  http://dbpedia.org/resource/Super_Ultra_Low_Emission_Vehicle\n  http://dbpedia.org/resource/Partial_zero-emissions_vehicle\n\nSee also http://en.wikipedia.org/wiki/Emissions_standard.\n\nUnfortunately, there are no DBPedia identifiers for the popular Euro 1 - Euro 5 standards available.\nIf you cannot find a suitable identifier in DBPedia, define your own as an instance of vso:EmissionStandardValue."@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "meetsEmissionStandard (0..*)"@en ;
        rdfs:range          vso:EmissionStandardValue ;
        rdfs:seeAlso        wikipedia:Emissions_standard ;
        rdfs:subPropertyOf  gr:qualitativeProductOrServiceProperty .

vso:1GLL  a                      vso:FuelQuantity ;
        rdfs:comment             "The fuel quantity of 1 US gallon"@en ;
        rdfs:isDefinedBy         <http://purl.org/vso/ns> ;
        rdfs:label               "1GLL (vso:FuelQuantity)"@en ;
        gr:hasUnitOfMeasurement  "GLL"^^xsd:string ;
        gr:hasValueFloat         "1.0"^^xsd:float .

vso:referenceSpeeds  a      owl:ObjectProperty ;
        rdfs:comment        "The interval of initial and target speed to which a vso:acceleration value refers"@en ;
        rdfs:domain         gr:QuantitativeValueFloat ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "referenceSpeeds (0..1)"@en ;
        rdfs:range          vso:SpeedInterval ;
        rdfs:subPropertyOf  gr:valueReference .

vso:4WD  a                vso:DriveWheelConfigurationValue ;
        rdfs:comment      "Four-wheel Drive is a transmission layout where the engine primarily drives two wheels with a part-time four-wheel drive capability."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Four-wheel drive (vso:DriveWheelConfigurationValue)"@en ;
        rdfs:seeAlso      wikipedia:Four-wheel_drive .

vso:feature  a              owl:ObjectProperty ;
        rdfs:comment        "Indicates a built-in feature, accessory, or component of the vehicle.\n\nUse DBPedia resources to indicate the features, if possible. Popular identifiers are\n  http://dbpedia.org/resource/Anti-lock_braking_system\n  http://dbpedia.org/resource/Electronic_stability_control\n  http://dbpedia.org/resource/Armrest\n  http://dbpedia.org/resource/Spoiler_%28automotive%29\n  http://dbpedia.org/resource/CD_Changer\n  http://dbpedia.org/resource/Cup_holder\n  http://dbpedia.org/resource/Tow_hitch\n  http://dbpedia.org/resource/Power_window\n  http://dbpedia.org/resource/Power_steering\n  http://dbpedia.org/resource/Air_conditioner\n  http://dbpedia.org/resource/Child_safety_lock\n  http://dbpedia.org/resource/Parking_sensors\n\nSee also http://en.wikipedia.org/wiki/Category:Automotive_accessories.\n\nIf you cannot find a suitable identifier in DBPedia, define your own as an instance of vso:FeatureValue."@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "feature (0..*)"@en ;
        rdfs:range          vso:FeatureValue ;
        rdfs:seeAlso        <http://dbpedia.org/resource/Category:Automotive_accessories> , <http://en.wikipedia.org/wiki/Category:Automotive_accessories> ;
        rdfs:subPropertyOf  gr:qualitativeProductOrServiceProperty .

vso:acceleration  a         owl:ObjectProperty ;
        rdfs:comment        "The time needed to accelerate the vehicle from a given start velocity to a given target velocity\nTypical unit code(s): SEC for seconds\n\nNote: There are unfortunately no standard unit codes for seconds/0..100 km/h or seconds/0..60 mph. Simply use \"SEC\" for seconds and indicate the velocities in the rdfs:label of the gr:QuantitativeValueFloat, or use vso:referenceSpeeds with vso:0to60MPH (0..60 mph) or vso:0to100KMH (0..100 km/h) to specify the reference speeds."@en ;
        rdfs:domain         [ a            owl:Class ;
                              owl:unionOf  ( vso:MotorizedRoadVehicle vso:Boat vso:Ship )
                            ] ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "acceleration (0..*)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:rentalUsage  a          owl:DatatypeProperty ;
        rdfs:comment        "Indicates whether the vehicle has been used for commercial rental. The legislation in many countries requires this information to be revealed when offering a car for sale. \n\nNote 1: This property should be used with gr:ActualProductOrServiceInstance (when offering a vehicle) or gr:ProductOrServicesSomeInstancesPlaceholder (when seeking a vehicle using gr:seeks) only.\nNote 2: This property is relevant mainly for offers to SELL the vehicle (gr:hasBusinessFunction gr:Sell)."@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "rentalUsage (0..1)"@en ;
        rdfs:range          xsd:boolean ;
        rdfs:subPropertyOf  gr:datatypeProductOrServiceProperty .

vso:height  a               owl:ObjectProperty ;
        rdfs:comment        "The outer height of the vehicle. Note that you can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges.\nTypical unit code(s): CMT for centimeters, MTR for meters, INH for inches, FOT for foot/feet"@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "height (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:Ship  a               owl:Class ;
        rdfs:comment      "A ship is a large watercraft that floats on water."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Ship (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Ship , wikipedia:Ship ;
        rdfs:subClassOf   vso:Watercraft .

vso:roofLoad  a             owl:ObjectProperty ;
        rdfs:comment        "The permited total weight of cargo and installations (e.g. a roof rack) on top of the vehicle\nTypical unit code(s): KGM for kilogram, LBR for pound\n\t\nNote 1: You can indicate additional information in the rdfs:label of the gr:QuantitativeValueFloat node.\nNote 2: You may also link to a gr:QualitativeValue node that provides additional information using gr:valueReference.\nNote 3: Note that you can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges."@en ;
        rdfs:domain         vso:MotorizedRoadVehicle ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "roofLoad (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:Bicycle  a            owl:Class ;
        rdfs:comment      "A bicycle or bike is a pedal-driven, human-powered, single-track vehicle, having two wheels attached to a frame, one behind the other. Some bicycles have a small combustion or electric engine that assists with the pedaling."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Bicycle (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Bicycle , wikipedia:Bicycle ;
        rdfs:subClassOf   vso:Vehicle .

vso:weight  a               owl:ObjectProperty ;
        rdfs:comment        "The weight of the empty vehicle (\"curb weight\" for cars), i.e. with standard equipment, all necessary operating consumables (e.g. motor oil and coolant), a full tank of fuel, while not loaded with either passengers or cargo\nTypical unit code(s): KGM for kilogram, LBR for pound\n\nNote 1: There are many varying definitions in place for specifying the weight of a vehicle. E.g., the many European Union car manufacturers include the weight of a 75 kilogram driver to follow European Directive 95/48/EC. Make sure to subtract those 75 kg when converting respective data.\nNote 2: You can indicate additional information in the rdfs:label of the gr:QuantitativeValueFloat node.\nNote 3: You may also link to a gr:QualitativeValue node that provides additional information using gr:valueReference.\nNote 4: Note that you can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges."@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "weight (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:seeAlso        wikipedia:Curb_weight ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:1LTR  a                      vso:FuelQuantity ;
        rdfs:comment             "The fuel quantity of 1 liter"@en ;
        rdfs:isDefinedBy         <http://purl.org/vso/ns> ;
        rdfs:label               "1LTR (vso:FuelQuantity)"@en ;
        gr:hasUnitOfMeasurement  "LTR"^^xsd:string ;
        gr:hasValueFloat         "1.0"^^xsd:float .

vso:cargoVolume  a          owl:ObjectProperty ;
        rdfs:comment        "The available volume for cargo or luggage. For automobiles, this is usually the trunk space.\nTypical unit code(s): LTR for liters, FTQ for cubic foot/feet\n\t\nNote: You can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges."@en ;
        rdfs:domain         [ a            owl:Class ;
                              owl:unionOf  ( vso:MotorizedRoadVehicle vso:Watercraft )
                            ] ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "cargoVolume (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:doors  a                owl:ObjectProperty ;
        rdfs:comment        "The number of doors\nTypical unit code(s): C62"@en ;
        rdfs:domain         vso:MotorizedRoadVehicle ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "doors (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueInteger ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:axles  a                owl:ObjectProperty ;
        rdfs:comment        "The number of axles \nTypical unit code(s): C62"@en ;
        rdfs:domain         vso:MotorizedRoadVehicle ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "axles (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueInteger ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:Canoe  a              owl:Class ;
        rdfs:comment      "A canoe (American English) or Canadian canoe (British English) is a small narrow boat, typically human-powered, though it may also be powered by sails or small electric or gas motors."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Canoe (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Canoe , wikipedia:Canoe ;
        rdfs:subClassOf   vso:Boat .

vso:Kayak  a              owl:Class ;
        rdfs:comment      "A kayak is a small human-powered watercraft that traditionally has a covered deck, and one or more cockpits, each seating one paddler who strokes a double-bladed paddle. In this ontology, kayaks are considered watercrafts of their own kind, not a subtype of vso:Boat."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Kayak (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Kayak , wikipedia:Kayak ;
        rdfs:subClassOf   vso:Watercraft .

vso:TransmissionTypeValue
        a                 owl:Class ;
        rdfs:comment      "A value indicating a type of transmission.\n\nYou can safely use any reasonable DBPedia URI, e.g. \n  http://dbpedia.org/resource/Manual_transmission\n  http://dbpedia.org/resource/Automatic_transmission"@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Transmission type value (the class of predefined values)"@en ;
        rdfs:subClassOf   gr:QualitativeValue .

vso:0to60MPH  a                  vso:SpeedInterval ;
        rdfs:comment             "The speed interval from 0 to 60 mph"@en ;
        rdfs:isDefinedBy         <http://purl.org/vso/ns> ;
        rdfs:label               "0to60MPH (vso:SpeedInterval)"@en ;
        gr:hasMaxValueFloat      "60.0"^^xsd:float ;
        gr:hasMinValueFloat      "0.0"^^xsd:float ;
        gr:hasUnitOfMeasurement  "HM"^^xsd:string .

vso:firstRegistration
        a                   owl:DatatypeProperty ;
        rdfs:comment        "The date of the first registration of the vehicle with the respective public authorities. This property should be used mainly with gr:ActualProductOrServiceInstance."@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "firstRegistration (0..1)"@en ;
        rdfs:range          xsd:date ;
        rdfs:subPropertyOf  gr:datatypeProductOrServiceProperty .

vso:width  a                owl:ObjectProperty ;
        rdfs:comment        "The outer width of the vehicle. Note that you can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges.\nTypical unit code(s): CMT for centimeters, MTR for meters, INH for inches, FOT for foot/feet"@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "width (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:fuelEconomy  a          owl:ObjectProperty ;
        rdfs:comment        "The distance traveled per unit of fuel used; most commonly miles per gallon (mpg) or kilometers per liter (km/L)\nTypical unit code(s): KMT for kilometers, SMI for statute miles\n\t\nNote 1: There are unfortunately no standard unit codes for miles per gallon or kilometers per liter. \nSimply use \"SMI\" for statute miles (common miles in the US and UK) or \"KMT\" for kilometers, and indicate \nthe fuel amount of reference in the rdfs:label of the gr:QuantitativeValueFloat, or use vso:referenceFuelQuantity with vso:1LTR (1 liter), vso:1GLL (1 US Gallon), or vso:1GLI (1 British Gallon). \nNote 2: In the absence of a vso:referenceFuelQuantity one may assume that the quantity of reference is 1 gallon for distances given in miles and 1 liter for distances given in kilometers. However, note that the original unit may be obscured by unit conversion services, so this heuristic is far from perfect.\nNote 3: There are two ways of indicating the fuel consumption, vso:fuelConsumption (e.g. 8 liters per 100 km) and vso:fuelEconomy (e.g. 30 miles per gallon). They are reciprocal.\nNote 4: Often, the absolute value is useful only when related to driving speed (\"at 80 km/h\") or usage pattern (\"city traffic\"). You can use gr:valueReference to link the value for the fuel economy to another value."@en ;
        rdfs:domain         vso:MotorizedRoadVehicle ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "fuelEconomy (0..*)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:seeAlso        wikipedia:Fuel_economy_in_automobiles ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:trailerWeight  a        owl:ObjectProperty ;
        rdfs:comment        "The permited weight of a trailer attached to the vehicle.\nTypical unit code(s): KGM for kilogram, LBR for pound\n\nNote 1: You can indicate additional information in the rdfs:label of the gr:QuantitativeValueFloat node.\nNote 2: You may also link to a gr:QualitativeValue node that provides additional information using gr:valueReference.\nNote 3: Note that you can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges."@en ;
        rdfs:domain         vso:MotorizedRoadVehicle ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "trailerWeight (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:seeAlso        wikipedia:Towing ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:SpeedInterval  a      owl:Class ;
        rdfs:comment      "A value indicating a speed interval, mostly for explaining vso:acceleration data"@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "SpeedInterval"@en ;
        rdfs:subClassOf   gr:QuantitativeValueFloat .

vso:payload  a              owl:ObjectProperty ;
        rdfs:comment        "The permited weight of passengers and cargo, EXCLUDING the weight of the empty vehicle\nTypical unit code(s): KGM for kilogram, LBR for pound\n\t\nNote 1: Many databases specify the permitted TOTAL weight instead, which is the sum of vso:weight and vso:payload.\nNote 2: You can indicate additional information in the rdfs:label of the gr:QuantitativeValueFloat node.\nNote 3: You may also link to a gr:QualitativeValue node that provides additional information using gr:valueReference.\nNote 4: Note that you can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges."@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "payload (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:weightTotal  a          owl:ObjectProperty ;
        rdfs:comment        "The permited total weight of the loaded vehicle, including passengers and cargo and the weight of the empty vehicle\nTypical unit code(s): KGM for kilogram, LBR for pound\n\t\nNote 1: You can indicate additional information in the rdfs:label of the gr:QuantitativeValueFloat node.\nNote 2: You may also link to a gr:QualitativeValue node that provides additional information using gr:valueReference.\nNote 3: Note that you can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges."@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "weightTotal (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:Right  a              vso:SteeringPositionValue ;
        rdfs:comment      "The steering position is on the right side of the vehicle (viewed from the main direction of driving). For cars this means that you drive on the left side of the road."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Right Hand Drive (vso:SteeringPositionValue)"@en .

vso:fuelTankVolume  a       owl:ObjectProperty ;
        rdfs:comment        "The volume of the fuel tank. If there are multiple tanks, this should indicate the total of all tanks.\nTypical unit code(s): LTR for liters, GLL of US gallons, GLI for UK / imperial gallons"@en ;
        rdfs:domain         [ a            owl:Class ;
                              owl:unionOf  ( vso:MotorizedRoadVehicle vso:Boat vso:Ship )
                            ] ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "fuelTankVolume (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:Rickshaw  a           owl:Class ;
        rdfs:comment      "A rickshaw (or ricksha) is a two-wheeled cart which seats one or two persons and is being drawn by a runner."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Rickshaw (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Rickshaw , wikipedia:Rickshaw ;
        rdfs:subClassOf   vso:Vehicle .

vso:gearsTotal  a           owl:ObjectProperty ;
        rdfs:comment        "The total number of forward and reverse gears available for the transmission system of the vehicle\nTypical unit code(s): C62"@en ;
        rdfs:domain         [ a            owl:Class ;
                              owl:unionOf  ( vso:MotorizedRoadVehicle vso:Boat vso:Ship vso:Bicycle vso:Quadracycle )
                            ] ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "gearsTotal (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueInteger ;
        rdfs:seeAlso        wikipedia:Gear ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:referenceDistance
        a                   owl:ObjectProperty ;
        rdfs:comment        "The distance to which a vso:fuelConsumption value refers"@en ;
        rdfs:domain         gr:QuantitativeValueFloat ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "referenceDistance (0..1)"@en ;
        rdfs:range          gr:QuantitativeValue ;
        rdfs:subPropertyOf  gr:valueReference .

vso:engineName  a           owl:DatatypeProperty ;
        rdfs:comment        "A short text indicating the engine(s) of the vehicle"@en ;
        rdfs:domain         [ a            owl:Class ;
                              owl:unionOf  ( vso:MotorizedRoadVehicle vso:Boat vso:Ship vso:Bicycle )
                            ] ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "engineName (0..*)"@en ;
        rdfs:range          rdfs:Literal ;
        rdfs:subPropertyOf  gr:datatypeProductOrServiceProperty .

vso:seatingCapacity  a      owl:ObjectProperty ;
        rdfs:comment        "The number of passengers that can be seated in the vehicle, both in terms of the physical space available, and in terms of limitations set by law\nTypical unit code(s): C62 for persons "@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "seatingCapacity (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueInteger ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:FWD  a                vso:DriveWheelConfigurationValue ;
        rdfs:comment      "Front-wheel drive is a transmission layout where the engine drives the front wheels."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Front-wheel drive, FWD (vso:DriveWheelConfigurationValue)"@en ;
        rdfs:seeAlso      wikipedia:Front-wheel_drive , <http://en.wikipedia.org/wiki/Drive_wheel#Front-wheel_drive> .

vso:Left  a               vso:SteeringPositionValue ;
        rdfs:comment      "The steering position is on the left side of the vehicle (viewed from the main direction of driving). For cars this means that you drive on the right side of the road."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Left Hand Drive (vso:SteeringPositionValue)"@en .

vso:VIN  a                  owl:DatatypeProperty ;
        rdfs:comment        "A unique serial number used by the automotive industry to identify individual motor vehicles"@en ;
        rdfs:domain         vso:MotorizedRoadVehicle ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "VIN [Vehicle Identification Number] (0..1)"@en ;
        rdfs:range          xsd:string ;
        rdfs:seeAlso        dbpedia:Vehicle_Identification_Number , wikipedia:Vehicle_Identification_Number ;
        rdfs:subPropertyOf  gr:serialNumber .

vso:SailingBoat  a        owl:Class ;
        rdfs:comment      "A sailboat or sailing boat is a boat propelled partly or entirely by sails."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Sailing boat (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Sailboat , wikipedia:Sailboat ;
        rdfs:subClassOf   vso:Boat .

vso:Boat  a               owl:Class ;
        rdfs:comment      "A boat is a watercraft of modest size designed to float or plane, to provide passage across water."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Boat (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Boat , wikipedia:Boat ;
        rdfs:subClassOf   vso:Watercraft .

vso:enginePower  a          owl:ObjectProperty ;
        rdfs:comment        "The power of the vehicle's engine. If there are multiple engines, simply attach this property multiple times.\nTypical unit code(s): KWT for kilowatt \n\t\nNote 1: There are many different ways of measuring an engine's power. For an overview, see  http://en.wikipedia.org/wiki/Horsepower#Engine_power_test_codes.\nNote 2: You can link to information about how the given value has been determined using the gr:valueReference property.\nNote 3: You can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges."@en ;
        rdfs:domain         [ a            owl:Class ;
                              owl:unionOf  ( vso:MotorizedRoadVehicle vso:Boat vso:Ship )
                            ] ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "enginePower (0..*)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:seeAlso        wikipedia:Horsepower ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

dc:contributor  a  owl:AnnotationProperty .

vso:ACRISSCode  a           owl:DatatypeProperty ;
        rdfs:comment        "The ACRISS Car Classification Code is a code used by many car rental companies, for classifying vehicles. ACRISS stands for Association of Car Rental Industry Systems and Standards."@en ;
        rdfs:domain         vso:MotorizedRoadVehicle ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "ACRISSCode (0..1)"@en ;
        rdfs:range          xsd:string ;
        rdfs:seeAlso        dbpedia:ACRISS_Car_Classification_Code , wikipedia:ACRISS_Car_Classification_Code ;
        rdfs:subPropertyOf  gr:datatypeProductOrServiceProperty .

vso:mileageFromOdometer
        a                   owl:ObjectProperty ;
        rdfs:comment        "The total distance travelled by the particular vehicle since its initial production, as read from its odometer\nTypical unit code(s): KMT for kilometers, SMI for statute miles\n\t\nNote: This property should be used with gr:ActualProductOrServiceInstance (when offering a vehicle) or gr:ProductOrServicesSomeInstancesPlaceholder (when seeking a vehicle using gr:seeks) only."@en ;
        rdfs:domain         vso:MotorizedRoadVehicle ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "mileageFromOdometer (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:Motorcycle  a         owl:Class ;
        rdfs:comment      "A motorcycle or motorbike is a single-track, two-wheeled motor vehicle."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Motorcycle (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Motorcycle , wikipedia:Motorcycle ;
        rdfs:subClassOf   vso:MotorizedRoadVehicle .

vso:EngineTypeValue  a    owl:Class ;
        rdfs:comment      "A value indicating an engine type.\n\nYou can safely use any reasonable DBPedia URI, e.g. \n  http://dbpedia.org/resource/Two-stroke_engine\n  http://dbpedia.org/resource/Four-stroke_engine\n  http://dbpedia.org/resource/Wankel_engine\n  http://dbpedia.org/resource/Electric_motor"@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Engine type value (the class of predefined values)"@en ;
        rdfs:subClassOf   gr:QualitativeValue .

vso:wheelbase  a            owl:ObjectProperty ;
        rdfs:comment        "The distance between the centers of the front and rear wheels\nTypical unit code(s): CMT for centimeters, MTR for meters, INH for inches, FOT for foot/feet"@en ;
        rdfs:domain         [ a            owl:Class ;
                              owl:unionOf  ( vso:MotorizedRoadVehicle vso:Bicycle vso:Quadracycle )
                            ] ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "wheelbase (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:seeAlso        wikipedia:Wheelbase ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:FuelTypeValue  a      owl:Class ;
        rdfs:comment      "A value indicating a type of fuel.\n \nYou can safely use any reasonable DBPedia URI, e.g. \n  http://dbpedia.org/resource/Gasoline\n  http://dbpedia.org/resource/Diesel\n  http://dbpedia.org/resource/Biodiesel\n  http://dbpedia.org/resource/Kerosene"@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Fuel type value (the class of predefined values)"@en ;
        rdfs:subClassOf   gr:QualitativeValue .

vso:MotorizedBicycle  a   owl:Class ;
        rdfs:comment      "A motorized bicycle is a bicycle with an attached motor used to power the vehicle, or to assist with pedaling."@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Motorized bicycle (gr:ProductOrService)"@en ;
        rdfs:seeAlso      dbpedia:Motorized_bicycle , wikipedia:Motorized_bicycle ;
        rdfs:subClassOf   vso:MotorizedRoadVehicle .

vso:engineType  a           owl:ObjectProperty ;
        rdfs:comment        "The type of engine or engines powering the vehicle. If there are multiple engines, simply attach this property multiple times.\n\nUse DBPedia resources to indicate the engine type. Popular identifiers are\n  http://dbpedia.org/resource/Two-stroke_engine\n  http://dbpedia.org/resource/Four-stroke_engine\n  http://dbpedia.org/resource/Wankel_engine\n  http://dbpedia.org/resource/Electric_motor\n\nIf you cannot find a suitable identifier in DBPedia, define your own as an instance of vso:EngineTypeValue."@en ;
        rdfs:domain         [ a            owl:Class ;
                              owl:unionOf  ( vso:MotorizedRoadVehicle vso:Boat vso:Ship )
                            ] ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "engineType (0..*)"@en ;
        rdfs:range          vso:EngineTypeValue ;
        rdfs:subPropertyOf  gr:qualitativeProductOrServiceProperty .

dc:rights  a    owl:AnnotationProperty .

vso:tongueWeight  a         owl:ObjectProperty ;
        rdfs:comment        "The permited vertical load (TWR) of a trailer attached to the vehicle. Also referred to as Tongue Load Rating (TLR) or Vertical Load Rating (VLR).\n\tTypical unit code(s): KGM for kilogram, LBR for pound\n\nNote 1: You can indicate additional information in the rdfs:label of the gr:QuantitativeValueFloat node.\nNote 2: You may also link to a gr:QualitativeValue node that provides additional information using gr:valueReference.\nNote 3: Note that you can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges."@en ;
        rdfs:domain         vso:MotorizedRoadVehicle ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "tongueWeight (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:seeAlso        wikipedia:Towing ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:bodyStyle  a            owl:ObjectProperty ;
        rdfs:comment        "Indicates the design and body style of the vehicle. \n\nUse DBPedia resources to indicate the features, if possible. Popular identifiers are\n  http://dbpedia.org/resource/Convertible\n  http://dbpedia.org/resource/Hatchback\n  http://dbpedia.org/resource/Station_wagon\n  http://dbpedia.org/resource/Sport_utility_vehicle\n  http://dbpedia.org/resource/Roadster\n\nSee also http://en.wikipedia.org/wiki/Car_body_styles.\n\nIf you cannot find a suitable identifier in DBPedia, define your own as an instance of vso:BodyStyleValue."@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "bodyStyle (0..1)"@en ;
        rdfs:range          vso:BodyStyleValue ;
        rdfs:seeAlso        wikipedia:Car_body_styles ;
        rdfs:subPropertyOf  gr:qualitativeProductOrServiceProperty .

vso:DriveWheelConfigurationValue
        a                 owl:Class ;
        rdfs:comment      "A value indicating which roadwheels will receive torque"@en ;
        rdfs:isDefinedBy  <http://purl.org/vso/ns> ;
        rdfs:label        "Drive wheel configuration value (the class of predefined values)"@en ;
        rdfs:subClassOf   gr:QualitativeValue .

dc:title  a     owl:AnnotationProperty .

vso:length  a               owl:ObjectProperty ;
        rdfs:comment        "The outer length of the vehicle. Note that you can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges.\nTypical unit code(s): CMT for centimeters, MTR for meters, INH for inches, FOT for foot/feet"@en ;
        rdfs:domain         gr:ProductOrService ;
        rdfs:isDefinedBy    <http://purl.org/vso/ns> ;
        rdfs:label          "length (0..1)"@en ;
        rdfs:range          gr:QuantitativeValueFloat ;
        rdfs:subPropertyOf  gr:quantitativeProductOrServiceProperty .

vso:1GLI  a                      vso:FuelQuantity ;
        rdfs:comment             "The fuel quantity of 1 British (imperial) gallon"@en ;
        rdfs:isDefinedBy         <http://purl.org/vso/ns> ;
        rdfs:label               "1GLI (vso:FuelQuantity)"@en ;
        gr:hasUnitOfMeasurement  "GLI"^^xsd:string ;
        gr:hasValueFloat         "1.0"^^xsd:float .
