                           owl:someValuesFrom  WeatherOntology:WeatherCondition
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  WeatherOntology:DewPointTemperature ;
                           owl:onProperty               WeatherOntology:hasWeatherPhenomenon
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  WeatherOntology:SolarIrradiance ;
                           owl:onProperty               WeatherOntology:hasWeatherPhenomenon
                         ] ;
        rdfs:subClassOf  [ a                         owl:Restriction ;
                           owl:onClass               [ a            owl:Class ;
                                                       owl:unionOf  ( time:Instant time:Interval )
                                                     ] ;
                           owl:onProperty            WeatherOntology:hasObservationTime ;
                           owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  WeatherOntology:CloudCover ;
                           owl:onProperty               WeatherOntology:hasWeatherPhenomenon
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
