	RegAp:name "isPartExpressionOf"@en ;
	RegAp:status <http://metadataregistry.org/uri/RegStatus/1001> .

frbrer:P2081 a owl:NamedIndividual ;
	skos:definition "Relates a manifestation to another manifestation, usually of the same expression, that represents the same intellectual or artistic content as the prior manifestation. The reproduction manifestation may or may not replicate the look and feel or other physical characteristics of the prior manifestation."@en ;
	RegAp:name "hasAReproductionManifestationFromManifestation"@en ;
	RegAp:status <http://metadataregistry.org/uri/RegStatus/1001> .

frbrer:P2082 a owl:NamedIndividual ;
