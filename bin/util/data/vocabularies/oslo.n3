	rdfs:isDefinedBy <http://purl.org/oslo/ns/localgov> ;
	rdfs:domain person:Person ;
	rdfs:range :Code ;
	dcterms:identifier "oslo:legalIncompetence" .

:hasPermission a rdf:Property ;
