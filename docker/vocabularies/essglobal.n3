	rdfs:domain <http://purl.org/essglobal/vocab/v1.0/SaleOption> .

<http://purl.org/essglobal/vocab/v1.0/salesMarginPercentage> a owl:DatatypeProperty ;
	rdfs:label "Sales Margin (percentage)"@en , "Margem de Vendas (percentagem)"@pt ;
	dc:description "Margin (on sales) is the difference between selling price and cost. This difference is typically expressed either as a percentage of selling price or on a per-unit basis. This attribute is the percentage sales margin."@en , "Margem (em vendas) é a diferença entre o preço de venda e o custo. Esta diferença é normalmente expressa como uma percentagem do preço de venda ou com base por unidade. Este atributo é a percentagem da margem de vendas."@pt ;
	rdfs:isDefinedBy <http://purl.org/essglobal/vocab/v1.0/> ;
	dc:issued "2014-02-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	dc:modified "2014-08-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	rdfs:range rdfs:Literal ;
	rdfs:domain <http://purl.org/essglobal/vocab/v1.0/SaleOption> .

<http://purl.org/essglobal/vocab/v1.0/salesMarginValue> a owl:DatatypeProperty ;
	rdfs:label "Sales Margin (value)"@en , "Margem de Vendas (valor)"@pt ;
	dc:description "Margin (on sales) is the difference between selling price and cost. This difference is typically expressed either as a percentage of selling price or on a per-unit basis. The present attribute is an absolute value for sales margin on each unit of the product or service offered in the location."@en , "Margem (em vendas) é a diferença entre o preço de venda e o custo. Esta diferença é normalmente expressa como uma percentagem do preço de venda ou com base por unidade. O presente atributo é um valor absoluto para a margem de vendas em cada uma das unidades do produto ou serviço oferecido no local."@pt ;
	rdfs:isDefinedBy <http://purl.org/essglobal/vocab/v1.0/> ;
	dc:issued "2014-02-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	dc:modified "2014-08-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
	rdfs:range rdfs:Literal ;
	rdfs:domain <http://purl.org/essglobal/vocab/v1.0/SaleOption> .

<http://purl.org/essglobal/vocab/v1.0/state> a owl:DatatypeProperty ;
	rdfs:label "State"@en , "Estado"@pt ;
	dc:description "The State where the Location of Sale is located."@en , "O Estado onde a Localização da Venda está situado."@pt ;
	rdfs:isDefinedBy <http://purl.org/essglobal/vocab/v1.0/> ;
	dc:issued "2014-02-06"^^<http://www.w3.org/2001/XMLSchema#date> ;
